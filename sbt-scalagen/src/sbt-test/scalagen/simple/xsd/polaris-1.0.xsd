<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
    <xs:simpleType name="uuid_t">
        <xs:annotation>
            <xs:documentation>Универсальный уникальный идентификатор
			</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:whiteSpace value="collapse"/>
            <xs:pattern value="[a-f0-9]{8}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{12}"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="exchange_participant_type_et">
        <xs:annotation>
            <xs:documentation>Типы участников обмена</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:whiteSpace value="preserve"/>
            <xs:enumeration value="REG">
                <xs:annotation>
                    <xs:documentation>Регистратор (реестродержатель)</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="SUBDIV">
                <xs:annotation>
                    <xs:documentation>Подразделение регистратора</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="id_t">
        <xs:annotation>
            <xs:documentation>Идентификатор объекта</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="id" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Идентификатор</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="organization" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Наименование организации, присвоившей идентификатор</xs:documentation>
                    <!-- ===================================================
Должен быть указан один из элементов (issuer или scheme_name)
Могут указываться коды, организаций, включенных в справочник Data Source Scheme, используемый SWIFT
=================================================== -->
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <!-- =========================================================================================== -->
    </xs:complexType>
    <xs:complexType name="exchange_participant_t">
        <xs:annotation>
            <xs:documentation>Информация об участнике обмена</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element minOccurs="1" name="id" type="uuid_t">
                <xs:annotation>
                    <xs:documentation>Идентификатор стороны обмена</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="participant_type" type="exchange_participant_type_et">
                <xs:annotation>
                    <xs:documentation>Тип участника обмена</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="name" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Наименование участника обмена
					</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="date_or_datetime_t">
        <xs:annotation>
            <xs:documentation>Дата или дата и время</xs:documentation>
        </xs:annotation>
        <xs:choice>
            <xs:element name="date" type="xs:date">
                <xs:annotation>
                    <xs:documentation>Дата</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="datetime" type="xs:dateTime">
                <xs:annotation>
                    <xs:documentation>Дата и время</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:choice>
    </xs:complexType>
    <xs:simpleType name="doc_type_et">
        <xs:annotation>
            <xs:documentation>Код типа документа</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="FORM">
                <xs:annotation>
                    <xs:documentation>Анкета зарегистрированного лица</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="FRPL">
                <xs:annotation>
                    <xs:documentation>Анкета залогодержателя</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="FRMN">
                <xs:annotation>
                    <xs:documentation>Анкета управляющей компании</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="APPL">
                <xs:annotation>
                    <xs:documentation>Заявление об открытии счета/ изменении анкеты</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="BANK">
                <xs:annotation>
                    <xs:documentation>Банковская карточка</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="PROX">
                <xs:annotation>
                    <xs:documentation>Доверенность</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="REPR">
                <xs:annotation>
                    <xs:documentation>Сведения об уполномоченном представителе</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="MNGR">
                <xs:annotation>
                    <xs:documentation>Документ о назначении руководителя</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="ATTC">
                <xs:annotation>
                    <xs:documentation>Приложение к анкете ЗЛ</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="BENF">
                <xs:annotation>
                    <xs:documentation>Сведения о бенефициарных владельцах и/или выгодоприобретателях</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="ACTU">
                <xs:annotation>
                    <xs:documentation>Заявление о подтверждении актуальности данных ЗЛ</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="PLDG">
                <xs:annotation>
                    <xs:documentation>Залоговое распоряжение</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="NOTR">
                <xs:annotation>
                    <xs:documentation>Запрос нотариуса</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="REQU">
                <xs:annotation>
                    <xs:documentation>Запрос на выдачу информации из реестра</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="ORDR">
                <xs:annotation>
                    <xs:documentation>Исполнительный лист</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="JUDG">
                <xs:annotation>
                    <xs:documentation>Решение суда</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="PASS">
                <xs:annotation>
                    <xs:documentation>Копия документа, удостоверяющего личность</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="TRAN">
                <xs:annotation>
                    <xs:documentation>Распоряжение о списании/зачислении ЦБ</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="COMB">
                <xs:annotation>
                    <xs:documentation>Распоряжение на объединение лицевых счетов</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="MEET">
                <xs:annotation>
                    <xs:documentation>Протокол собрания</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="BLOK">
                <xs:annotation>
                    <xs:documentation>Распоряжение на блокирование</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="UNBL">
                <xs:annotation>
                    <xs:documentation>Распоряжение на отмену блокирования</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="UNPL">
                <xs:annotation>
                    <xs:documentation>Распоряжение о прекращении залога</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="PLMF">
                <xs:annotation>
                    <xs:documentation>Распоряжение о внесении изменений в залог</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="PLDL">
                <xs:annotation>
                    <xs:documentation>Распоряжение о передаче права залога</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="INHE">
                <xs:annotation>
                    <xs:documentation>Свидетельство о праве на наследство</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="CREG">
                <xs:annotation>
                    <xs:documentation>Свидетельство о регистрации</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="COGR">
                <xs:annotation>
                    <xs:documentation>Свидетельство о внесении в ЕГРЮЛ</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="LOGR">
                <xs:annotation>
                    <xs:documentation>Лист записи в ЕГРЮЛ</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="CHAR">
                <xs:annotation>
                    <xs:documentation>Устав</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="SIGN">
                <xs:annotation>
                    <xs:documentation>Карточка с образцами подписей</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="CLOS">
                <xs:annotation>
                    <xs:documentation>Заявление на закрытие лицевого счета</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="OTHR">
                <xs:annotation>
                    <xs:documentation>Другой документ</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="doc_type_t">
        <xs:annotation>
            <xs:documentation>Тип входящего документа</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="doc_type_code" type="doc_type_et">
                <xs:annotation>
                    <xs:documentation>Код типа документа</xs:documentation>
                    <!-- ===================================================
Код вида обременения, должен принимать значения из фиксированного перечня. 
=================================================== -->
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="narrative" type="xs:string">
                <xs:annotation>
                    <!-- ===================================================
Заполняется обязательно, только в случае, если значением кода вида обременения указано OTHR
=================================================== -->
                    <xs:documentation>Наименование документа, если его тип - OTHR</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <!-- =========================================================================================== -->
    </xs:complexType>
    <xs:complexType name="doc_link_t">
        <xs:annotation>
            <xs:documentation>Ссылка на документ</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element minOccurs="0" name="out_doc_num" nillable="false" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Исходящий номер документа у отправителя</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="1" name="out_doc_date" type="xs:date">
                <xs:annotation>
                    <xs:documentation>Дата создания документа у отправителя</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="in_doc_num" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Входящий регистрационный номер</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="in_reg_date" type="xs:date">
                <xs:annotation>
                    <xs:documentation>Дата регистрации входящего документа</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="ta_doc_num" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Входящий номер документа у трансфер-агента</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="ta_doc_date" type="xs:date">
                <xs:annotation>
                    <xs:documentation>Дата регистрации документа у трансфер-агента</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <!-- =========================================================================================== -->
        <xs:attribute name="uid" type="uuid_t">
            <xs:annotation>
                <xs:documentation>Идентификатор сообщения</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="legal_ogrn_t">
        <xs:annotation>
            <xs:documentation>ОГРН юридического лица</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="ogrn" type="xs:string">
                <xs:annotation>
                    <xs:documentation>ОГРН</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ogrn_date" nillable="true" type="xs:date">
                <xs:annotation>
                    <xs:documentation>дата выдачи ОГРН</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="ogrn_place" type="xs:string">
                <xs:annotation>
                    <xs:documentation>орган, осуществивший регистрацию</xs:documentation>
                    <!-- ===================================================
Полное официальное наименование  юридического лица - органа, осуществившего регистрацию
=================================================== -->
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <!-- =========================================================================================== -->
    </xs:complexType>
    <xs:simpleType name="legal_reg_doc_type_et">
        <xs:annotation>
            <xs:documentation>Код типа документа о регистрации юридического лица</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="LICS">
                <xs:annotation>
                    <xs:documentation>Лицензия</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="REGN">
                <xs:annotation>
                    <xs:documentation>Свидетельство о регистрации </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="INCR">
                <xs:annotation>
                    <xs:documentation>Сертификат об инкорпорации</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="USTV">
                <xs:annotation>
                    <xs:documentation>Устав</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="UKZP">
                <xs:annotation>
                    <xs:documentation>Указ Президента</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="PSTN">
                <xs:annotation>
                    <xs:documentation>Постановление</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="OTHR">
                <xs:annotation>
                    <xs:documentation>Другой документ</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="NULL">
                <xs:annotation>
                    <xs:documentation>Не указан</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
        <!-- =========================================================================================== -->
    </xs:simpleType>
    <xs:complexType name="legal_doc_type_t">
        <xs:annotation>
            <xs:documentation>Тип документа регистрации юридического лица</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="juridical_doc_type" type="legal_reg_doc_type_et">
                <xs:annotation>
                    <xs:documentation>тип документа о регистрации юридического лица</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="narrative" type="xs:string">
                <xs:annotation>
                    <xs:documentation>текстовое описание типа документа</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <!-- =========================================================================================== -->
    </xs:complexType>
    <xs:complexType name="legal_reg_doc_t">
        <xs:annotation>
            <xs:documentation>Документ регистрации юридического лица</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="reg_doc_type" type="legal_doc_type_t">
                <xs:annotation>
                    <xs:documentation>тип документа о регистрации юридического лица</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="reg_num" nillable="true" type="xs:string">
                <xs:annotation>
                    <xs:documentation>номер регистрации</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="reg_series" type="xs:string">
                <xs:annotation>
                    <xs:documentation>серия документа</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="reg_date" nillable="true" type="xs:date">
                <xs:annotation>
                    <xs:documentation>дата регистрации</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="reg_place" type="xs:string">
                <xs:annotation>
                    <xs:documentation>орган, осуществивший регистрацию</xs:documentation>
                    <!-- ===================================================
Полное официальное наименование  юридического лица - органа, осуществившего регистрацию
=================================================== -->
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <!-- =========================================================================================== -->
    </xs:complexType>
    <xs:complexType name="legal_registration_t">
        <xs:annotation>
            <xs:documentation>Регистрация юридического лица</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element minOccurs="0" name="ogrn" nillable="false" type="legal_ogrn_t">
                <xs:annotation>
                    <xs:documentation>ОГРН </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="reg_doc" type="legal_reg_doc_t">
                <xs:annotation>
                    <xs:documentation>документ гос.регистрации (для юридических лиц, зарегистрированных до 01.07.2002 и нерезидентов)</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="legal_info_t">
        <xs:annotation>
            <xs:documentation>Характеристика юридического лица</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="juridical_name" nillable="false">
                <xs:annotation>
                    <xs:documentation>полное наименование юридического лица</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="1"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="juridical_registration" type="legal_registration_t">
                <xs:annotation>
                    <xs:documentation>информация о регистрации юридического лица</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:simpleType name="account_type_et">
        <xs:annotation>
            <xs:documentation>Вид счета</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="ISSUE">
                <xs:annotation>
                    <xs:documentation>Эмиссионный счет</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="EMITENT">
                <xs:annotation>
                    <xs:documentation>Лицевой счет эмитента</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="OWNER">
                <xs:annotation>
                    <xs:documentation>Владелец</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="NOMINEE">
                <xs:annotation>
                    <xs:documentation>Номинальный держатель</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="TRUSTEE">
                <xs:annotation>
                    <xs:documentation>Доверительный управляющий</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="PLEDGEE">
                <xs:annotation>
                    <xs:documentation>Залогодержатель</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="UNKNOWN">
                <xs:annotation>
                    <xs:documentation>Счет неустановленных лиц</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="NOTARY">
                <xs:annotation>
                    <xs:documentation>Нотариус</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="RIGHTS">
                <xs:annotation>
                    <xs:documentation>Доверительный управляющий правами</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="TREASURY">
                <xs:annotation>
                    <xs:documentation>Казначейский счет эмитента</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="DEPOSIT">
                <xs:annotation>
                    <xs:documentation>Депозитный счет</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="NDCD">
                <xs:annotation>
                    <xs:documentation>Номинальный держатель центрального депозитария</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="FORNOMINEE">
                <xs:annotation>
                    <xs:documentation>Иностранный номинальный держатель</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="FORAUTHOR">
                <xs:annotation>
                    <xs:documentation>Иностранный уполномоченный держатель</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="DEPOPROG">
                <xs:annotation>
                    <xs:documentation>Счет депозитарных программ</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="individual_info_t">
        <xs:annotation>
            <xs:documentation>Характеристика физического лица</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="individual_name" nillable="false">
                <xs:annotation>
                    <xs:documentation>фамилия, имя, отчество </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="1"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="individual_document" type="individual_document_t">
                <xs:annotation>
                    <xs:documentation>документ, удостоверяющий личность</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
