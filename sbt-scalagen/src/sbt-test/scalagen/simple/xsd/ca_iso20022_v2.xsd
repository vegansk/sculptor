	<xsd:complexType name="XtnsnCorporateActionNarrative">
		<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">НРД: Расширения для Сообщение о корпоративном действии в свободном тексте / NSD: Additional for CorporateActionNarrative</xsd:documentation></xsd:annotation>
