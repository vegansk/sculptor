		<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Физическое лицо (17) / IndividualPerson (17)</xsd:documentation></xsd:annotation>
		<xsd:sequence>
			
			<xsd:element name="BirthNm" type="Max35Text">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Фамилия / Birth Name</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element name="GvnNm" type="Max35Text" minOccurs="0">
