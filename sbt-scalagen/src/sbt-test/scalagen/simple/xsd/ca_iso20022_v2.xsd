	<xsd:complexType name="CorporateActionOptionAdditionalNSDR">
		<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Дополнительная информация для Варианты корпоративного действия / Additional information for CorporateActionDetails</xsd:documentation></xsd:annotation>
		<xsd:sequence>
			<xsd:element name="OptnNb" type="Exact3NumericText">
