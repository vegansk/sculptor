			</xsd:element>
			<xsd:element name="DtdDt" type="ISODate" minOccurs="0">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Дата начала начисления процентов / DatedDate</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element name="ConvsDt" type="ISODate" minOccurs="0">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Дата конвертации / ConversionDate</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element name="PrvsFctr" type="RateFormat3Choice" minOccurs="0">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Предыдущее значение фактора / PreviousFactor</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element name="NxtFctr" type="RateFormat3Choice" minOccurs="0">
