	<xsd:simpleType name="NewSecuritiesIssuanceType3Code">
		<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Справочник: Код типа выпуска новых ценных бумаг (3) / Dictionary: NewSecuritiesIssuanceTypeCode (3)</xsd:documentation></xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="DEFE">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Индикатор аннулируемой ценной бумаги / Defeased Security Indicator</xsd:documentation></xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="NDEF">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Индикатор не аннулируемой ценной бумаги / Non Defeased Security Indicator</xsd:documentation></xsd:annotation>
