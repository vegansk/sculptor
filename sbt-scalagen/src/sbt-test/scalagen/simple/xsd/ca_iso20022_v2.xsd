		<xsd:sequence>
			<xsd:element name="Id" type="Max35Text">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Идентификатор / Identification</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element name="Issr" type="Max35Text">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Организация, присвоившая код / Issuer</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element name="SchmeNm" type="Max35Text" minOccurs="0">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Наименование системы кодировки / SchemeName</xsd:documentation></xsd:annotation>
			</xsd:element>
		</xsd:sequence>
