		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="NELC">
				<xsd:annotation>
					<xsd:documentation source="MDR" xml:lang="RusEng">Невозможность обработки валюты / NonEligibleCurrency
Расчеты по валюте не могут быть проведены Центральным депозитарием.</xsd:documentation></xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="ACLI">
				<xsd:annotation>
					<xsd:documentation source="MDR" xml:lang="RusEng">Ограничения по счету / AccountLimitation
Ограничения по счету участника.</xsd:documentation></xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="ONEL">
				<xsd:annotation>
					<xsd:documentation source="MDR" xml:lang="RusEng">Другие причины / OtherNonEligibility
Другия причины невозможности обработки.</xsd:documentation></xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="NoReasonCode">
		<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Справочник: Код отсутсвия причины / Dictionary: NoReasonCode</xsd:documentation></xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="NORE">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Причина не указана / NoReason</xsd:documentation></xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
