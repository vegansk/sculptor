		<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Варианты ценной бумаги (23) / SecuritiesOption (23)</xsd:documentation></xsd:annotation>
		<xsd:sequence>
			
			<xsd:element name="MaxExrcblQty" type="FinancialInstrumentQuantity19Choice" minOccurs="0">
