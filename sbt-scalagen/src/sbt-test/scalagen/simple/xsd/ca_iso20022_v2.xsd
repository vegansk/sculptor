	<xsd:simpleType name="ISINOct2015Identifier">
		<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">ISIN / ISIN</xsd:documentation></xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[A-Z]{2,2}[A-Z0-9]{9,9}[0-9]{1,1}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="LEIIdentifier">
		<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Идентификатор LEI / LEI Identifier</xsd:documentation></xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[A-Z0-9]{18,18}[0-9]{2,2}"/>
