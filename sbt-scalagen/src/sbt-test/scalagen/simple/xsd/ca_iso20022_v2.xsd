			</xsd:element>
			<xsd:element name="PtyCtctNrrtv" type="UpdatedAdditionalInformation2" minOccurs="0">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Контактная информация / PartyContactNarrative</xsd:documentation></xsd:annotation>
			</xsd:element>
