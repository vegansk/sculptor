	<xsd:complexType name="FailingReason8">
		<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Причина невозможности расчетов (8) / Failing Reason (8)</xsd:documentation></xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Cd" type="FailingReason8Choice">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Код / Code</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element name="AddtlRsnInf" type="Max210Text" minOccurs="0">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Дополнительная информация о причине / AdditionalReasonInformation</xsd:documentation></xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="FailingReason8Choice">
		<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Выбор причины невозможности расчетов (8) / Failing Reason Choice (8)</xsd:documentation></xsd:annotation>
		<xsd:choice>
			<xsd:element name="Cd" type="FailingReason2Code">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Код / Code</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element name="Prtry" type="GenericIdentification30">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Собственный код / Proprietary</xsd:documentation></xsd:annotation>
			</xsd:element>
		</xsd:choice>
	</xsd:complexType>
	<xsd:complexType name="FailingStatus10Choice">
		<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Выбор статуса невозможности расчетов (10) / Failing Status Choice (10)</xsd:documentation></xsd:annotation>
		<xsd:choice>
			<xsd:element name="NoSpcfdRsn" type="NoReasonCode">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Причина не указана / NoSpecifiedReason</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element name="Rsn" type="FailingReason8" maxOccurs="unbounded">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Причина / Reason</xsd:documentation></xsd:annotation>
			</xsd:element>
		</xsd:choice>
	</xsd:complexType>
	<xsd:complexType name="GenericIdentification30">
		<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Основная идентификация (30) / Generic Identification (30)</xsd:documentation></xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Id" type="Exact4AlphaNumericText">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Идентификатор / Identification</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element name="Issr" type="Max35Text">
				<xsd:annotation>
					<xsd:documentation source="MDR" xml:lang="RusEng">Организация, присвоившая код / Issuer
Указывается константа "NSDR".</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element name="SchmeNm" type="Max35Text" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation source="MDR" xml:lang="RusEng">Наименование системы кодировки / SchemeName
Не используется</xsd:documentation></xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="GenericIdentification36">
