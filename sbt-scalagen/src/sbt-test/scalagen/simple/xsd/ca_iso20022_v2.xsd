	<xsd:complexType name="MeetingResultDisseminationV04">
		<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Содержание Сообщение об итогах собрания / Content of message Meeting Result Dissemination</xsd:documentation></xsd:annotation>
