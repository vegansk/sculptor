			<xsd:element name="Role" type="AgentRole1Code" minOccurs="0">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Роль / Role</xsd:documentation></xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="IssuerOfferorTaxabilityIndicator1Choice">
