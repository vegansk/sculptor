		<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Идентификатор документа (14) / DocumentIdentification (14)</xsd:documentation></xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Id" type="DocumentIdentification1Choice">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Идентификатор / Identification</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element name="DocNb" type="DocumentNumber1Choice" minOccurs="0">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Определение номера / DocumentNumber</xsd:documentation></xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DocumentIdentification15">
		<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Идентификатор документа (15) / DocumentIdentification (15)</xsd:documentation></xsd:annotation>
		<xsd:sequence>
			
			<xsd:element name="Id" type="Max35Text">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Идентификатор / Identification</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element name="LkgTp" type="ProcessingPosition1Choice" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation source="MDR" xml:lang="RusEng">Признак типа связок / LinkageType
Является технической информацией и на экранную форму не выводится.</xsd:documentation></xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DocumentIdentification1Choice">
		<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Выбор идентификатора документа (1) / DocumentIdentificationChoice (1)</xsd:documentation></xsd:annotation>
		<xsd:choice>
			
			<xsd:element name="AcctSvcrDocId" type="Max35Text">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Референс организации, обслуживающей счет / AccountServicerDocumentIdentification</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element name="AcctOwnrDocId" type="Max35Text">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Референс владельца счета / AccountOwnerDocumentIdentification</xsd:documentation></xsd:annotation>
			</xsd:element>
		</xsd:choice>
	</xsd:complexType>
	<xsd:complexType name="ElectionTypeFormat1Choice">
		<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Выбор кода типа альтернативного варианта (1) / ElectionTypeFormatChoice (1)</xsd:documentation></xsd:annotation>
		<xsd:choice>
			
			<xsd:element name="Cd" type="ElectionMovementType2Code">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Код / Code</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element name="Prtry" type="GenericIdentification20">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Собственный код / Proprietary</xsd:documentation></xsd:annotation>
			</xsd:element>
		</xsd:choice>
	</xsd:complexType>
	<xsd:complexType name="EligibilityDates1">
		<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Описание даты фиксации права / Defines the dates determining eligibility.</xsd:documentation></xsd:annotation>
		<xsd:sequence>
			<xsd:element name="EntitlmntFxgDt" type="ISODate">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Дата фиксации права / EntitlementFixingDate</xsd:documentation></xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="EligiblePosition3">
		<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Позиция для голосовании (3) / EligiblePosition (3)</xsd:documentation></xsd:annotation>
		<xsd:sequence>
			
			<xsd:element name="AcctId" type="Max35Text" minOccurs="0">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Идентификатор счета / AccountIdentification</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element name="AcctOwnr" type="PartyIdentification9Choice" minOccurs="0">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Владелец счета / AccountOwner</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element name="HldgBal" type="HoldingBalance6" minOccurs="0" maxOccurs="10">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Остаток по счету / HoldingBalance</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element name="RghtsHldr" type="PartyIdentification9Choice" minOccurs="0" maxOccurs="10">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Держатель прав / RightsHolder</xsd:documentation></xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="EligiblePosition4">
		<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Позиция для голосовании (4) / EligiblePosition (4)</xsd:documentation></xsd:annotation>
		<xsd:sequence>
			<xsd:element name="AcctId" type="Max35Text" minOccurs="0">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Идентификатор счета / AccountIdentification</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element name="AcctOwnr" type="PartyIdentification39" minOccurs="0">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Владелец счета / AccountOwner</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element name="HldgBal" type="HoldingBalance6" minOccurs="0" maxOccurs="10">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Остаток по счету / HoldingBalance</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element name="RghtsHldr" type="PartyIdentification39" minOccurs="0" maxOccurs="10">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Держатель прав / RightsHolder</xsd:documentation></xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Entitlement1Choice">
		<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Предоставляемое право для голосования (выбор) / Entitlement1Choice</xsd:documentation></xsd:annotation>
		<xsd:choice>
			
			<xsd:element name="EntitlmntRatio" type="DecimalNumber">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Количество голосов на одну ценную бумагу / EntitlementRatio</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element name="EntitlmntDesc" type="Max35Text">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Описание метода расчета количества голосов на одну ценную бумагу / EntitlementDescription</xsd:documentation></xsd:annotation>
			</xsd:element>
		</xsd:choice>
	</xsd:complexType>
	<xsd:complexType name="EntitlementAssessment2">
		<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Характеристика права на голосование (2) / EntitlementAssessment2</xsd:documentation></xsd:annotation>
		<xsd:sequence>
			
			<xsd:element name="SctiesBlckgDdln" type="DateFormat2Choice" minOccurs="0">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Последний срок блокирования ЦБ (устанавливается посредником) / SecuritiesBlockingDeadline</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element name="SctiesBlckgSTPDdln" type="DateFormat2Choice" minOccurs="0">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Последний срок рынка для блокирования ЦБ (устанавливается эмитентом, режим STP) / SecuritiesBlockingSTPDeadline</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element name="SctiesBlckgMktDdln" type="DateFormat2Choice" minOccurs="0">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Последний срок рынка для блокирования ЦБ (устанавливается эмитентом) / SecuritiesBlockingMarketDeadline</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element name="SctiesBlckgPrdEndDt" type="ISODateTime" minOccurs="0">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Срок окончания периода блокирования ценных бумаг / SecuritiesBlockingPeriodEndDate</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element name="EntitlmntFxgDt" type="DateFormat3Choice" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation source="MDR" xml:lang="RusEng">Дата фиксации прав (дата закрытия реестра) / EntitlementFixingDate
Для BMET в сообщении о КД дата фиксации должна обязательно присутствовать и быть равна Дате собрания минус 7 рабочих дней</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element name="RegnSctiesDdln" type="DateFormat2Choice" minOccurs="0">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Последний срок регистрации ЦБ (установлен посредником) / RegistrationSecuritiesDeadline</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element name="RegnSctiesSTPDdln" type="DateFormat2Choice" minOccurs="0">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Последний срок регистрации ЦБ (установлен посредником, режим STP) / RegistrationSecuritiesSTPDeadline</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element name="RegnSctiesMktDdln" type="DateFormat2Choice" minOccurs="0">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Последний срок рынка для регистрации ЦБ (установлен эмитентом) / RegistrationSecuritiesMarketDeadline</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element name="RegnPrtcptnDdln" type="DateFormat2Choice" minOccurs="0">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Последний срок регистрации участников (установлен посредником) / RegistrationParticipationDeadline</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element name="RegnPrtcptnSTPDdln" type="DateFormat2Choice" minOccurs="0">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Последний срок регистрации участников (установлен посредником, режим STP) / RegistrationParticipationSTPDeadline</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element name="RegnPrtcptnMktDdln" type="DateFormat2Choice" minOccurs="0">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Последний срок рынка для регистрации участников (установлен эмитентом) / RegistrationParticipationMarketDeadline</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element name="Entitlmnt" type="Entitlement1Choice" minOccurs="0">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Право (количество голосов на одну ценую бумагу) / Entitlement</xsd:documentation></xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Event1">
		<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Информация о событии / EventInformation</xsd:documentation></xsd:annotation>
		<xsd:sequence>
			<xsd:element name="EvtCd" type="Max4AlphaNumericText">
				<xsd:annotation>
					<xsd:documentation source="MDR" xml:lang="RusEng">Код события / EventCode
Заполняется одним из кодов.
Код - расшифровка кода
PACK - Принято / Accepted
RCIS - Принято эмитентом или регистратором / ReceivedByIssuerOrRegistrar
NOIN - Не получено / NotReceived
COMP - Завершено / Completed
NWID - Новый код события / New Action Code 
CHAN - Изменение параметров КД / CA Terms Changed</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element name="EvtParam" type="Max35Text" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation source="MDR" xml:lang="RusEng">Параметр события / EventParameter
Если EvtCd=NWID (Новый код события / New Action Code), то поле EvtParam содержит {ID ОСА, присвоенный НРД} ({присвоенный НРД код событию})
В остальных случаях не используется.</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element name="EvtDesc" type="Max350Text" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation source="MDR" xml:lang="RusEng">Описание события / EventDescription
Не используется.</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element name="EvtTm" type="ISODateTime" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation source="MDR" xml:lang="RusEng">Время события / EventTime
Локальные дата и время присвоения статуса, например, дата и время присвоения кода КД.</xsd:documentation></xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="EventProcessingStatus1Choice">
		<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Выбор статуса обработки события (1) /EventProcessingStatusChoice (1)</xsd:documentation></xsd:annotation>
		<xsd:choice>
			<xsd:element name="Cmplt" type="NoSpecifiedReason1">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Завершено/Complete</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element name="Rcncld" type="NoSpecifiedReason1">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Выверка проведена / Reconciled</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element name="Pdg" type="PendingStatus2Choice">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Не завершено / Pending</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element name="PrtrySts" type="ProprietaryStatusAndReason1">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Собственный статус / ProprietaryStatus</xsd:documentation></xsd:annotation>
			</xsd:element>
		</xsd:choice>
	</xsd:complexType>
	<xsd:complexType name="Extension2">
		<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Блок для расширений (2) / Extension (2)</xsd:documentation></xsd:annotation>
		<xsd:sequence>
			<xsd:element name="PlcAndNm" type="Max350Text" minOccurs="0">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Место и наименование / PlaceAndName</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element name="XtnsnEnvlp" type="ExtensionEnvelope1">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Конверт для расширения / Extension Envelope</xsd:documentation></xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ExtensionEnvelope1">
		<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Конверт для расширения (1) / ExtensionEnvelope (1)</xsd:documentation></xsd:annotation>
		<xsd:sequence>
			<xsd:any namespace="##any" processContents="lax">
				
			</xsd:any>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="InformationTypeFormat4Choice">
		<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Выбор формата типа информации (4) / InformationTypeFormat4Choice (4)</xsd:documentation></xsd:annotation>
		<xsd:choice>
			<xsd:element name="Cd" type="CorporateActionInformationType1Code">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Код / Code</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element name="Prtry" type="GenericIdentification30">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Собственный код / Proprietary</xsd:documentation></xsd:annotation>
			</xsd:element>
		</xsd:choice>
	</xsd:complexType>
	<xsd:complexType name="FailingReason4">
		<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Причина невозможности расчетов (4) / Failing Reason (4)</xsd:documentation></xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Cd" type="FailingReason4Choice">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Код / Code</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element name="AddtlRsnInf" type="Max210Text" minOccurs="0">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Дополнительная информация о причине / AdditionalReasonInformation</xsd:documentation></xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="FailingReason4Choice">
		<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Выбор причины невозможности расчетов (4) / Failing Reason Choice (4)</xsd:documentation></xsd:annotation>
		<xsd:choice>
			<xsd:element name="Cd" type="FailingReason3Code">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Код / Code</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element name="Prtry" type="GenericIdentification20">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Собственный статус / ProprietaryStatus</xsd:documentation></xsd:annotation>
			</xsd:element>
		</xsd:choice>
	</xsd:complexType>
	<xsd:complexType name="FailingStatus4Choice">
		<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Выбор статуса невозможности расчетов (4) / Failing Status Choice (4)</xsd:documentation></xsd:annotation>
		<xsd:choice>
			<xsd:element name="NoSpcfdRsn" type="NoReasonCode">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Причина не указана / NoSpecifiedReason</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element name="Rsn" type="FailingReason4" maxOccurs="unbounded">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Причина / Reason</xsd:documentation></xsd:annotation>
			</xsd:element>
		</xsd:choice>
	</xsd:complexType>
	<xsd:complexType name="FATCABeneficiaryDisclosureDetailsNSDR">
		<xsd:annotation>
			<xsd:documentation source="MDR" xml:lang="RusEng">Раскрытие информации о бенефициарах для WHS / WHSBeneficiaryDisclosureDetails (NSDR)
Используется для отправки Отчет клиента для налогового раскрытия по US-бумагам,  используется для иностранных КД DVCA, INTR.</xsd:documentation></xsd:annotation>
		<xsd:sequence>
			<xsd:element name="OwndSctiesQty" type="FinancialInstrumentQuantity1Choice">
				<xsd:annotation>
					<xsd:documentation source="MDR" xml:lang="RusEng">Количество ценных бумаг владельца / OwnedSecuritiesQuantity
В текущей реализации используется только блок Unit</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element name="ClntId" type="Max35Text">
				<xsd:annotation>
					<xsd:documentation source="MDR" xml:lang="RusEng">Код клиента / ClientIdentification
Депозитарный код зарегистрированного с W8TX бенефициара</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element name="ClntNm" type="Max350Text">
				<xsd:annotation>
					<xsd:documentation source="MDR" xml:lang="RusEng">Наименование клиента  / Client Name
Наименование клиента из анкеты W8TX или W9TX</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element name="IntrmyPties" type="IntermediaryIdentificationNSDR" minOccurs="0" maxOccurs="5">
				<xsd:annotation>
					<xsd:documentation source="MDR" xml:lang="RusEng">Цепочка посредников / IntermediaryParties
В список не включается депонент НРД, который составляет Отчет клиента для раскрытия по US-бумагам.
В списке посредники указываются в последовательности от депонента НРД к бенефициару, количество блоков должно соответствовать количеству посредников в списке. Нумерация начинается с 1. Пропуск номеров не допускается.</xsd:documentation></xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="FATCABeneficiaryIntermediaryDetailsNSDR">
		<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Информация по разделам, где есть бенефициар и посредники / BeneficiaryIntermediaryDetails</xsd:documentation></xsd:annotation>
		<xsd:sequence>
			<xsd:element name="SubAcctCd" type="Max17Text">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Код раздела / SubAccountCode</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element name="SubAcctId" type="Max8NumericText">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Идентификатор раздела   / SubAccountIdentification</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element name="OwndSctiesQty" type="FinancialInstrumentQuantity1Choice">
				<xsd:annotation>
					<xsd:documentation source="MDR" xml:lang="RusEng">Количество ценных бумаг владельца / OwnedSecuritiesQuantity
В текущей реализации используется только блок Unit</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element name="ClntId" type="Max35Text">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Код клиента / ClientIdentification</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element name="ClntIdFATCA" type="FATCAClientIdentificationNSDR" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation source="MDR" xml:lang="RusEng">Идентификация клиента по анкете FATCA / ClientIdentificationFATCA
В блоке ClntIdFATCA указывается наименование клиента в соответствии с анкетой FATCA и Статус по Главе 4 FATCA. Отсутствие этих данных в Уведомлении об остатках (Запросе посредника на налоговое раскрытие) по US-бумагам означает отсутствие в НРД сведений.</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element name="WhldgTaxRate" type="PercentageRate">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Ставка удерживаемого налога / WithholdingTaxRate</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element name="LstIntrmyId" type="IntermediaryIdentificationNSDR">
				<xsd:annotation>
					<xsd:documentation source="MDR" xml:lang="RusEng">Идентификация последнего посредника / LastIntermediaryIdentification
