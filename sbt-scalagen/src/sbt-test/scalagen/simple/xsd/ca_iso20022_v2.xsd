			</xsd:element>
			<xsd:element name="PrcgId" type="Max35Text" minOccurs="0">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Идентификатор процеса / Processing Identification</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element name="AddtlInf" type="Max350Text" minOccurs="0">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Дополнительная информация / AdditionalInformation</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element name="AltrnId" type="AlternatePartyIdentification2" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Дополнительный идентификатор / Alternate Identification</xsd:documentation></xsd:annotation>
