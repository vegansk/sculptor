	<xsd:complexType name="GenericIdentification78">
		<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Основная идентификация (78) / Generic Identification (78)</xsd:documentation></xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Tp" type="GenericIdentification30">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Тип / Type</xsd:documentation></xsd:annotation>
			</xsd:element>
			<xsd:element name="Id" type="Max35Text" minOccurs="0">
				<xsd:annotation><xsd:documentation source="MDR" xml:lang="RusEng">Идентификатор / Identification</xsd:documentation></xsd:annotation>
			</xsd:element>
