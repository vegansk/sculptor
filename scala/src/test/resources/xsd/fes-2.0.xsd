<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
    <xs:annotation>
        <xs:documentation>Форматы Элдис-Софт версии 2.0</xs:documentation>
    </xs:annotation>
    <xs:element name="DOCUMENT">
        <xs:annotation>
            <xs:documentation>Документ</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="message_header" type="message_header_t">
                    <xs:annotation>
                        <xs:documentation>Заголовок сообщения</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="document_header" type="document_header_t">
                    <xs:annotation>
                        <xs:documentation>Заголовок документа</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="document_sender" type="document_sender_t" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Отправитель документа</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" name="document_person" type="document_person_t">
                    <xs:annotation>
                        <xs:documentation>Субъект, в отношении которого подан документ</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" name="document_appendices" type="document_appendices_t">
                    <xs:annotation>
                        <xs:documentation>Список приложений к документу</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" name="transfer_agent_document_info"
                    type="transfer_agent_document_info_t">
                    <xs:annotation>
                        <xs:documentation>Информация о передаче документа через ТА</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" name="issuer_info" type="issuer_info_t">
                    <xs:annotation>
                        <xs:documentation>Эмитент</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="content" type="document_content_t">
                    <xs:annotation>
                        <xs:documentation>Содержимое документа</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" name="notifications" type="notifications_info_t">
                    <xs:annotation>
                        <xs:documentation>Уведомления</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" name="services" type="services_t">
                    <xs:annotation>
                        <xs:documentation>Список услуг</xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:simpleType name="uuid_t">
        <xs:annotation>
            <xs:documentation>Универсальный уникальный идентификатор
			</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:whiteSpace value="collapse"/>
            <xs:pattern value="[a-f0-9]{8}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{12}"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="id_t">
        <xs:annotation>
            <xs:documentation>Идентификатор объекта</xs:documentation>
        </xs:annotation>
        <xs:restriction base="uuid_t"/>
    </xs:simpleType>
    <xs:simpleType name="exchange_participant_type_et">
        <xs:annotation>
            <xs:documentation>Типы участников обмена</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="Registrar">
                <xs:annotation>
                    <xs:documentation>Регистратор (реестродержатель)</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="RegistrarSubdivision">
                <xs:annotation>
                    <xs:documentation>Подразделение регистратора</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="exchange_participant_t">
        <xs:annotation>
            <xs:documentation>Информация об участнике обмена</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element minOccurs="1" name="id" type="id_t">
                <xs:annotation>
                    <xs:documentation>Идентификатор стороны обмена</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="participant_type" type="exchange_participant_type_et">
                <xs:annotation>
                    <xs:documentation>Тип участника обмена</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="name" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Наименование участника обмена
					</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="message_header_t">
        <xs:annotation>
            <xs:documentation>Заголовок сообщения</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element fixed="fes-2.0" name="version" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Версия схемы</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="id" type="id_t">
                <xs:annotation>
                    <xs:documentation>Уникальный идентификатор сообщения</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="sender" type="exchange_participant_t">
                <xs:annotation>
                    <xs:documentation>Отправитель сообщения</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="recipient" type="exchange_participant_t">
                <xs:annotation>
                    <xs:documentation>Получатель сообщения</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:simpleType name="document_type_et">
        <xs:annotation>
            <xs:documentation>Код типа документа</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="ApplicationToOpenIndividualAccount">
                <xs:annotation>
                    <xs:documentation>Заявление на открытие лицевого счета (ФЛ)</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="ApplicationToOpenLegalAccount">
                <xs:annotation>
                    <xs:documentation>Заявление на открытие лицевого счета (ЮЛ)</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="ApplicationToOpenCoownershipAccount">
                <xs:annotation>
                    <xs:documentation>Заявление на открытие счета ОС/ОДС</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="FormOfIndividualShareholder">
                <xs:annotation>
                    <xs:documentation>Анкета зарегистрированного лица (ФЛ)</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="FormOfLegalShareholder">
                <xs:annotation>
                    <xs:documentation>Анкета зарегистрированного лица (ЮЛ)</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="OrderToTransferShares">
                <xs:annotation>
                    <xs:documentation>Распоряжение о списании/зачислении ценных бумаг</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="OrderToConsolidateAccounts">
                <xs:annotation>
                    <xs:documentation>Распоряжение на объединение лицевых счетов</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="RequestForInformation">
                <xs:annotation>
                    <xs:documentation>Запрос на выдачу информации из реестра</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="OrderToBlock">
                <xs:annotation>
                    <xs:documentation>Распоряжение на блокирование</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="OrderToUnblock">
                <xs:annotation>
                    <xs:documentation>Распоряжение на отмену блокирования</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="OrderToPledge">
                <xs:annotation>
                    <xs:documentation>Залоговое распоряжение</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="OrderToChangePledge">
                <xs:annotation>
                    <xs:documentation>Распоряжение о внесении изменений в условия залога</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="OrderToUnpledge">
                <xs:annotation>
                    <xs:documentation>Распоряжение о прекращении залога</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="OrderToTransferPledgeRights">
                <xs:annotation>
                    <xs:documentation>Распоряжение о переуступке права требования по залогу</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Proxy">
                <xs:annotation>
                    <xs:documentation>Доверенность</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="InformationOnRepresentative">
                <xs:annotation>
                    <xs:documentation>Сведения об уполномоченном представителе</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="DocumentOfHeadAppointment">
                <xs:annotation>
                    <xs:documentation>Документ о назначении руководителя</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="FormOfShareholderAppendix">
                <xs:annotation>
                    <xs:documentation>Приложение к анкете зарегистрированного лица</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="InformationOnBeneficiaries">
                <xs:annotation>
                    <xs:documentation>Сведения о бенефициарных владельцах и/или выгодоприобретателях</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Other">
                <xs:annotation>
                    <xs:documentation>Другой тип документа</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="document_type_t">
        <xs:annotation>
            <xs:documentation>Тип входящего документа</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="code" type="document_type_et">
                <xs:annotation>
                    <xs:documentation>Код типа документа</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="name" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Наименование документа, если его тип - Other</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="base_document_link_t">
        <xs:annotation>
            <xs:documentation>Ссылка на документ-основание</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="id" type="id_t">
                <xs:annotation>
                    <xs:documentation>Идентификатор сообщения</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="out_num" nillable="false" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Исходящий номер документа у отправителя</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="out_date" type="xs:date">
                <xs:annotation>
                    <xs:documentation>Дата создания документа у отправителя</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="1" name="in_num" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Входящий регистрационный номер</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="1" name="in_date" type="xs:date">
                <xs:annotation>
                    <xs:documentation>Дата регистрации входящего документа</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:sequence minOccurs="0">
                <xs:element name="ta_num" type="xs:string">
                    <xs:annotation>
                        <xs:documentation>Входящий номер документа у трансфер-агента</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="ta_date" type="xs:date">
                    <xs:annotation>
                        <xs:documentation>Дата регистрации документа у трансфер-агента</xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:sequence>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="document_header_t">
        <xs:annotation>
            <xs:documentation>Заголовок документа</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="type" type="document_type_t">
                <xs:annotation>
                    <xs:documentation>Тип документа</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="out_num" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Исходящий номер у отправителя</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="out_date" type="xs:date"/>
            <xs:element minOccurs="0" name="base_document" type="base_document_link_t">
                <xs:annotation>
                    <xs:documentation>Ссылка на инициирующий документ</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:simpleType name="person_kind_et">
        <xs:annotation>
            <xs:documentation>Вид субъекта</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="Account">
                <xs:annotation>
                    <xs:documentation>Зарегистрированное лицо</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Issuer">
                <xs:annotation>
                    <xs:documentation>Эмитент</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Other">
                <xs:annotation>
                    <xs:documentation>Другие</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="ogrn_t">
        <xs:annotation>
            <xs:documentation>ОГРН юридического лица</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="ogrn" type="xs:string">
                <xs:annotation>
                    <xs:documentation>ОГРН</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="date" type="xs:date">
                <xs:annotation>
                    <xs:documentation>Дата выдачи</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="place">
                <xs:annotation>
                    <xs:documentation>Место выдачи</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:simpleType name="legal_registration_document_type_et">
        <xs:annotation>
            <xs:documentation>Код типа документа о регистрации юридического лица</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="License">
                <xs:annotation>
                    <xs:documentation>Лицензия</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="RegistrationCertificate">
                <xs:annotation>
                    <xs:documentation>Свидетельство о регистрации</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="CertificateOfIncorporation">
                <xs:annotation>
                    <xs:documentation>Сертификат об инкорпорации</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Charter">
                <xs:annotation>
                    <xs:documentation>Устав</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="PresidentialDecree">
                <xs:annotation>
                    <xs:documentation>Указ Президента</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Resolution">
                <xs:annotation>
                    <xs:documentation>Постановление</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Other">
                <xs:annotation>
                    <xs:documentation>Другой документ</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="legal_registration_document_type_t">
        <xs:annotation>
            <xs:documentation>Тип документа регистрации юридического лица</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="code" type="legal_registration_document_type_et"/>
            <xs:element minOccurs="0" name="name" type="xs:string"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="legal_registration_document_t">
        <xs:annotation>
            <xs:documentation>Документ регистрации юридического лица</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="type" type="legal_registration_document_type_t">
                <xs:annotation>
                    <xs:documentation>Тип документа о регистрации юридического лица</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="series" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Серия документа</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="num" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Номер документа</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="date" type="xs:date">
                <xs:annotation>
                    <xs:documentation>Дата документа</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="place" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Орган, осуществивший регистрацию</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="legal_registration_t">
        <xs:annotation>
            <xs:documentation>Данные регистрации юридического лица</xs:documentation>
        </xs:annotation>
        <xs:choice>
            <xs:element name="ogrn" type="ogrn_t">
                <xs:annotation>
                    <xs:documentation>ОГРН</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="registration_document" type="legal_registration_document_t">
                <xs:annotation>
                    <xs:documentation>Документ гос.регистрации (для юридических лиц, зарегистрированных до 01.07.2002 и нерезидентов)</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:choice>
    </xs:complexType>
    <xs:complexType name="legal_info_t">
        <xs:annotation>
            <xs:documentation>Характеристика юридического лица</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="name" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Полное наименование</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="registration" type="legal_registration_t">
                <xs:annotation>
                    <xs:documentation>Информация о регистрации юридического лица</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:simpleType name="country_et">
        <xs:annotation>
            <xs:documentation>Общероссийский классификатор стран мира</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="AB">
                <xs:annotation>
                    <xs:documentation>АБХАЗИЯ</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="AU">
                <xs:annotation>
                    <xs:documentation>АВСТРАЛИЯ</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="AT">
                <xs:annotation>
                    <xs:documentation>АВСТРИЯ</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="AZ">
                <xs:annotation>
                    <xs:documentation>АЗЕРБАЙДЖАН</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="AL">
                <xs:annotation>
                    <xs:documentation>АЛБАНИЯ</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="DZ">
                <xs:annotation>
                    <xs:documentation>АЛЖИР</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="AS">
                <xs:annotation>
                    <xs:documentation>АМЕРИКАНСКОЕ САМОА</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="AI">
                <xs:annotation>
                    <xs:documentation>АНГИЛЬЯ</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="AO">
                <xs:annotation>
                    <xs:documentation>АНГОЛА</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="AD">
                <xs:annotation>
                    <xs:documentation>АНДОРРА</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="AQ">
                <xs:annotation>
                    <xs:documentation>АНТАРКТИДА</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="AG">
                <xs:annotation>
                    <xs:documentation>АНТИГУА И БАРБУДА</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="AR">
                <xs:annotation>
                    <xs:documentation>АРГЕНТИНА</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="AM">
                <xs:annotation>
                    <xs:documentation>АРМЕНИЯ</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="AW">
                <xs:annotation>
                    <xs:documentation>АРУБА</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="AF">
                <xs:annotation>
                    <xs:documentation>АФГАНИСТАН</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="BS">
                <xs:annotation>
                    <xs:documentation>БАГАМЫ</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="BD">
                <xs:annotation>
                    <xs:documentation>БАНГЛАДЕШ</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="BB">
                <xs:annotation>
                    <xs:documentation>БАРБАДОС</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
