	<xsd:simpleType name="corporate_action_event_et">
		<xsd:annotation>
			<xsd:documentation>Код корпоративного действия/ события</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="BIDS">
				<xsd:annotation>
					<xsd:documentation>Оферта /предложение Эмитента о выкупе ценных бумаг</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="BONU">
				<xsd:annotation>
					<xsd:documentation>Размещение дополнительных выпусков акций путем распределения среди акционеров</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="BPUT">
				<xsd:annotation>
					<xsd:documentation>Досрочное погашение облигаций по требованию владельца</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="CONV">
				<xsd:annotation>
					<xsd:documentation>Конвертация конвертируемых ценных бумаг</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="DECR">
				<xsd:annotation>
					<xsd:documentation>Конвертация акций  в акции с меньшей   номинальной стоимостью</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="DRAW">
				<xsd:annotation>
					<xsd:documentation>Погашение части номинальной стоимости</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="DVCA">
