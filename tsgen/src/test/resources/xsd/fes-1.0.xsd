<xsd:schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<!--Настоящая схема данных описывает форматы взаимодействия между регистратором (реестродержателем) и лицами, с которыми у него возможен электронный документооборот (далее по тексту - абонентами ЭДО). В качестве абонентов ЭДО могут выступать эмитенты, номинальные держатели, акционеры, трансфер-агенты. Форматы содержат описание электронных документов на проведение операций в реестре и запросов на выдачу информации из реестра. -->
	<!---->
	<!--................ ДОКУМЕНТЫ ...................................................................................................................................................-->
	<!---->
	<xsd:element name="FORM_OF_SHAREHOLDERS">
		<xsd:annotation>
			<xsd:documentation>Анкета зарегистрированного лица</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="doc_t">
					<xsd:sequence>
						<xsd:element name="issuer" type="issuer_info_t">
							<xsd:annotation>
								<xsd:documentation>Информация об эмитенте</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="form_for" type="form_for_et">
							<xsd:annotation>
								<xsd:documentation>Цель подачи анкеты: открытие счета или изменение анкетных данных</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="account_form" type="operation_form_t">
							<xsd:annotation>
								<xsd:documentation>Данные анкеты</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="scan_info" type="scan_info_t" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Скан образ бумажного оригинала документа (возможен у документа ТА)</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="appendix" type="doc_app_t" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Список приложений к документу (возможен у документа ТА)</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="add_info" type="any_t" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Дополнительная информация</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="FORM_OF_COOWNERS">
		<xsd:annotation>
			<xsd:documentation>Анкета совладельца</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="doc_t">
					<xsd:sequence>
						<xsd:element name="issuer" type="issuer_info_t">
							<xsd:annotation>
								<xsd:documentation>Информация об эмитенте</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="form_for" type="coowner_form_for_et">
							<xsd:annotation>
								<xsd:documentation>Цель подачи анкеты: открыть новый счет ОС, добавить совладельца к счету ОС, или изменение анкеты совладельца</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="coowners_data" type="content_form_of_shareholders_t" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Реквизиты счета общей собственности,  необходимые для открытия нового счета ОС</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="coowner_form" type="operation_coowner_t">
							<xsd:annotation>
								<xsd:documentation>Данные анкеты совладельца</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="scan_info" type="scan_info_t" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Скан образ бумажного оригинала документа (возможен у документа ТА)</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="appendix" type="doc_app_t" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Список приложений к документу (возможен у документа ТА)</xsd:documentation>
