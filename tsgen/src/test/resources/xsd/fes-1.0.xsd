				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="INDA">
				<xsd:annotation>
					<xsd:documentation>Междепозитарный договор</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="NCBO">
				<xsd:annotation>
					<xsd:documentation>Поручение клиента</xsd:documentation>
				</xsd:annotation>
