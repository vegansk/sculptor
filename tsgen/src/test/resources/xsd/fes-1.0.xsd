		<!-- =========================================================================================== -->
	</xsd:simpleType>
	<xsd:simpleType name="block_reason_et">
		<xsd:annotation>
			<xsd:documentation>Основание блокирования ценных бумаг</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="ARST">
				<xsd:annotation>
					<xsd:documentation>Арест ЦБ</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="DEAT">
				<xsd:annotation>
					<xsd:documentation>В связи со смертью</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="BUY8">
				<xsd:annotation>
					<xsd:documentation>При выкупе ЦБ (ст.84.8)</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="THIR">
				<xsd:annotation>
					<xsd:documentation>Обременение правами третьих лиц</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="OTHR">
				<xsd:annotation>
					<xsd:documentation>По иным основаниям</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
		<!-- =========================================================================================== -->
	</xsd:simpleType>
	<xsd:simpleType name="blockoper_type_et">
		<xsd:annotation>
			<xsd:documentation>Вид блокирования операций по счету</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="4"/>
			<xsd:maxLength value="4"/>
			<xsd:enumeration value="ALLS">
				<xsd:annotation>
					<xsd:documentation>Блокирование всех операций</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="DELI">
				<xsd:annotation>
					<xsd:documentation>Блокирование расходных операций</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="DERE">
				<xsd:annotation>
					<xsd:documentation>Блокирование приходно-расходных операций</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="STOC">
				<xsd:annotation>
					<xsd:documentation>Блокирование всех операций с ЦБ</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="DEPL">
				<xsd:annotation>
					<xsd:documentation>Блокирование расходных операций и залогов</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
		<!-- =========================================================================================== -->
	</xsd:simpleType>
	<xsd:simpleType name="condition_type_et">
		<xsd:annotation>
			<xsd:documentation>Варианты выполнения условия залога</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="no">
				<xsd:annotation>
					<xsd:documentation>Не выполняется</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="all">
				<xsd:annotation>
					<xsd:documentation>На всё количество</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="part">
				<xsd:annotation>
					<xsd:documentation>На указанное количество</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="contract_et">
