	<xsd:simpleType name="result_reconciliation_et">
		<xsd:annotation>
			<xsd:documentation>Результат сверки по документу </xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="GOOD">
				<xsd:annotation>
					<xsd:documentation>Документ найден, сверка выполнена</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="MANUAL">
				<xsd:annotation>
					<xsd:documentation>Документ найден, сверка выполнена вручную</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="DIVER">
				<xsd:annotation>
					<xsd:documentation>Документ найден, но есть расхождения в реквизитах документа</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="SPARE">
				<xsd:annotation>
					<xsd:documentation>Документ, пришедший в запросе на сверку от регистратора, не найден у трансфер-агента (лишний документ)</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="MISSI">
				<xsd:annotation>
					<xsd:documentation>Документ, имеющийся  у трансфер-агента, отсутствует в запросе регистратора на сверку</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
		<!-- =========================================================================================== -->
	</xsd:simpleType>
	<xsd:simpleType name="result_reconciliation_total_et">
		<xsd:annotation>
			<xsd:documentation>Итоговый результат сверки по запросу </xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="GOOD">
				<xsd:annotation>
					<xsd:documentation>Cверка выполнена, несоответствий в журналах регистратора и трансфер-агента не выявлено</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="DIVER">
				<xsd:annotation>
					<xsd:documentation>Выявлены несоответствия в журналах регистратора и трансфер-агента</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
		<!-- =========================================================================================== -->
	</xsd:simpleType>
