	<xsd:element name="REQUEST_FOR_STATEMENT_IN_XML_FORMAT">
		<xsd:annotation>
			<xsd:documentation>Распоряжение на выдачу информации по лицевому счету в xml-формате</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="doc_t">
					<xsd:sequence>
						<xsd:element name="issuer" type="issuer_info_t">
							<xsd:annotation>
								<xsd:documentation>Информация об эмитенте</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="report_info">
							<xsd:annotation>
								<xsd:documentation>Данные запроса</xsd:documentation>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="account_ident" type="account_ident_t">
										<xsd:annotation>
											<xsd:documentation>Зарегистрированное лицо, по счету которого запрашивается отчет</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="authorised_person" type="authorised_person_info_t" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>Уполномоченный представитель ЗЛ (обязателен для юридических лиц)</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="statement_type" type="statement_type_et">
										<xsd:annotation>
											<xsd:documentation>Тип запрашиваемой информации</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:choice>
										<xsd:element name="operation" type="operation_brief_t">
											<xsd:annotation>
												<xsd:documentation>Описание операции для запроса уведомления об операции</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="statement">
											<xsd:annotation>
												<xsd:documentation>Данные для запроса выписки, справки об операциях, справки о наличии ЦБ</xsd:documentation>
											</xsd:annotation>
											<xsd:complexType>
												<xsd:sequence>
													<xsd:element name="date_or_period" type="date_or_period_t">
														<xsd:annotation>
															<xsd:documentation>Дата, на которую запрашивается справка, или диапазон дат</xsd:documentation>
														</xsd:annotation>
													</xsd:element>
													<xsd:element name="security" type="security_info_t" minOccurs="0">
														<xsd:annotation>
															<xsd:documentation>Идентификатор ЦБ</xsd:documentation>
														</xsd:annotation>
													</xsd:element>
													<xsd:element name="quantity" type="units_or_fraction_t" minOccurs="0">
														<xsd:annotation>
															<xsd:documentation>Количество ЦБ</xsd:documentation>
														</xsd:annotation>
													</xsd:element>
												</xsd:sequence>
											</xsd:complexType>
										</xsd:element>
									</xsd:choice>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="comment" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Комментарий</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="scan_info" type="scan_info_t" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Скан образ бумажного оригинала документа (возможен у документа ТА)</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="appendix" type="doc_app_t" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Список приложений к документу (возможен у документа ТА)</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="add_info" type="any_t" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Дополнительная информация</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="REQUEST_FOR_REPORT_IN_EXTERNAL_FORMAT">
		<xsd:annotation>
			<xsd:documentation>Запрос на отчет из системы ведения реестра во внешнем формате</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="doc_t">
					<xsd:sequence>
						<xsd:element name="issuer" type="issuer_info_t">
							<xsd:annotation>
								<xsd:documentation>Информация об эмитенте</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="report_info">
							<xsd:annotation>
								<xsd:documentation>Данные запроса</xsd:documentation>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="account_ident" type="account_ident_t" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>Зарегистрированное лицо, по счету которого запрашивается отчет</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="authorised_person" type="authorised_person_info_t" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>Уполномоченный представитель ЗЛ (обязателен для юридических лиц)</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="report_type">
										<xsd:annotation>
											<xsd:documentation>Тип запрашиваемого отчета</xsd:documentation>
										</xsd:annotation>
										<xsd:complexType>
											<xsd:sequence>
												<xsd:element name="id_report" type="id_t">
													<xsd:annotation>
														<xsd:documentation>Код отчета по внутренней кодировке регистратора</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="narrative" type="xsd:string" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>Наименование отчета</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
											</xsd:sequence>
										</xsd:complexType>
									</xsd:element>
									<xsd:element name="date_or_period" type="date_or_period_t" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>Дата, на которую запрашивается отчет, или период дат</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:choice minOccurs="0">
										<xsd:element name="operation" type="operation_brief_t">
											<xsd:annotation>
												<xsd:documentation>Описание операции для запроса уведомления об операции</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="statement_avai">
											<xsd:annotation>
												<xsd:documentation>Данные для запроса справки о наличии ЦБ</xsd:documentation>
											</xsd:annotation>
											<xsd:complexType>
												<xsd:sequence>
													<xsd:element name="security" type="security_info_t">
														<xsd:annotation>
															<xsd:documentation>Идентификатор ЦБ</xsd:documentation>
														</xsd:annotation>
													</xsd:element>
													<xsd:element name="quantity" type="units_or_fraction_t">
														<xsd:annotation>
															<xsd:documentation>Количество ЦБ</xsd:documentation>
														</xsd:annotation>
													</xsd:element>
												</xsd:sequence>
											</xsd:complexType>
										</xsd:element>
										<xsd:element name="filter_by_percent">
											<xsd:annotation>
												<xsd:documentation>Данные для запроса списка ЗЛ - выборка по процентам</xsd:documentation>
											</xsd:annotation>
											<xsd:complexType>
												<xsd:sequence>
													<xsd:element name="percent_kind" type="filter_by_percent_et">
														<xsd:annotation>
															<xsd:documentation>Тип выборки</xsd:documentation>
														</xsd:annotation>
													</xsd:element>
													<xsd:element name="percent" type="xsd:decimal">
														<xsd:annotation>
															<xsd:documentation>Процент больше либо равный...
														</xsd:documentation>
														</xsd:annotation>
													</xsd:element>
												</xsd:sequence>
											</xsd:complexType>
										</xsd:element>
									</xsd:choice>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="report_mode" type="report_mode_et">
							<xsd:annotation>
								<xsd:documentation>Форма предоставления отчета (бумажный оригинал, электронный документ)</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:sequence minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Если форма предоставления - бумажный оригинал</xsd:documentation>
							</xsd:annotation>
							<xsd:element name="e_copy" type="yes_no_t">
								<xsd:annotation>
									<xsd:documentation>Признак передачи электронной копии документа</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="report_delivery">
								<xsd:annotation>
									<xsd:documentation>Данные о выдаче ответного исходящего документа </xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="delivery_place" type="delivery_place_et" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation>Место выдачи - трансфер-агент или регистратор (заполнять для запросов от трансфер-агента)</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="delivery_method" type="delivery_method_et">
											<xsd:annotation>
												<xsd:documentation>Способ выдачи </xsd:documentation>
											</xsd:annotation>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
						<xsd:element name="comment" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Комментарий</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="scan_info" type="scan_info_t" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Скан образ бумажного оригинала документа (возможен у документа ТА)</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="appendix" type="doc_app_t" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Список приложений к документу (возможен у документа ТА)</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="add_info" type="any_t" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Дополнительная информация</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="STATEMENT_OF_HOLDINGS">
		<xsd:annotation>
			<xsd:documentation>Выписка из реестра / Справка на дату / Справка о наличии</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="out_doc_t">
					<xsd:sequence>
						<xsd:element name="registrar" type="registrar_info_t">
							<xsd:annotation>
								<xsd:documentation>Данные о регистраторе</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="issuer" type="issuer_info_t">
							<xsd:annotation>
								<xsd:documentation>Информация об эмитенте</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="account_ident" type="account_ident_t">
							<xsd:annotation>
								<xsd:documentation>Зарегистрированное лицо, по счету которого предоставляется отчет</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="statement_type" type="statement_type_et">
							<xsd:annotation>
								<xsd:documentation>Тип выписки</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="statement_date" type="date_or_datetime_t">
							<xsd:annotation>
								<xsd:documentation>Дата, на которую составлена выписка</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="security_data">
							<xsd:annotation>
								<xsd:documentation>Данные выписки (справки) о ценных бумагах на счете</xsd:documentation>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="security_info" type="xsd:string">
										<xsd:annotation>
											<xsd:documentation>Преамбула к описанию ЦБ на счете, либо информация об отсутствии ЦБ на счете  </xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="security_balance" type="security_balance_t" minOccurs="0" maxOccurs="unbounded">
										<xsd:annotation>
											<xsd:documentation>Баланс по ценной бумаге </xsd:documentation>
										</xsd:annotation>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="make_datetime" type="xsd:dateTime">
							<xsd:annotation>
								<xsd:documentation>Дата/ время составления выписки</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="add_info" type="any_t" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Дополнительная информация</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="STATEMENT_OF_TRANSACTIONS">
		<xsd:annotation>
			<xsd:documentation>Справка об операциях по счету</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="out_doc_t">
					<xsd:sequence>
						<xsd:element name="registrar" type="registrar_info_t">
							<xsd:annotation>
								<xsd:documentation>Данные о регистраторе</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="issuer" type="issuer_info_t">
							<xsd:annotation>
								<xsd:documentation>Информация об эмитенте</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="account_ident" type="account_ident_t">
							<xsd:annotation>
								<xsd:documentation>Зарегистрированное лицо, по счету которого предоставляется отчет</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="beg_date" type="xsd:date" nillable="true">
							<xsd:annotation>
								<xsd:documentation>Дата начала периода, за который сформирована справка</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="end_date" type="xsd:date" nillable="true">
							<xsd:annotation>
								<xsd:documentation>Дата конца периода</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="security_data">
							<xsd:annotation>
								<xsd:documentation>Состояние счета на начало и конец периода</xsd:documentation>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="security_info" type="xsd:string">
										<xsd:annotation>
											<xsd:documentation>Преамбула к описанию ЦБ на счете, либо информация об отсутствии ЦБ на счете</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="security_balance" minOccurs="0" maxOccurs="unbounded">
										<xsd:annotation>
											<xsd:documentation>Баланс ЦБ на начало и конец периода</xsd:documentation>
										</xsd:annotation>
										<xsd:complexType>
											<xsd:sequence>
												<xsd:element name="security" type="security_info_t">
													<xsd:annotation>
														<xsd:documentation>Ценная бумага</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="opening_balance" type="security_quantity_t">
													<xsd:annotation>
														<xsd:documentation>Количество ЦБ на счете на начало периода</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="closing_balance" type="security_quantity_t">
													<xsd:annotation>
														<xsd:documentation>Количество ЦБ на счете на конец периода</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
											</xsd:sequence>
										</xsd:complexType>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="operation_data">
							<xsd:annotation>
								<xsd:documentation>Список операций за указанный период</xsd:documentation>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="operation_info" type="xsd:string">
										<xsd:annotation>
											<xsd:documentation>Преамбула к перечню операций за период, либо информация об отсутствии операций  </xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="operation" type="operation_t" minOccurs="0" maxOccurs="unbounded">
										<xsd:annotation>
											<xsd:documentation>Список операций за период</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="make_datetime" type="xsd:dateTime">
							<xsd:annotation>
								<xsd:documentation>Дата/ время составления отчета</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="add_info" type="any_t" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Дополнительная информация</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="STATEMENT_OF_CONFIRMED_TRANSACTION">
		<xsd:annotation>
			<xsd:documentation>Уведомление о проведении операции</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="out_doc_t">
					<xsd:sequence>
						<xsd:element name="registrar" type="registrar_info_t">
							<xsd:annotation>
								<xsd:documentation>Данные о регистраторе</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="issuer" type="issuer_info_t">
							<xsd:annotation>
								<xsd:documentation>Информация об эмитенте</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="account_ident" type="account_ident_t">
							<xsd:annotation>
								<xsd:documentation>Зарегистрированное лицо, по счету которого предоставляется отчет</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="operation" type="operation_t">
							<xsd:annotation>
								<xsd:documentation>Описание операции</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="make_datetime" type="xsd:dateTime">
							<xsd:annotation>
								<xsd:documentation>Дата/ время составления отчета</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="add_info" type="any_t" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Дополнительная информация</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="STATEMENT_OF_REJECTED_TRANSACTION">
		<xsd:annotation>
			<xsd:documentation>Уведомление об отказе в проведении операции</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="out_doc_t">
					<xsd:sequence>
						<xsd:element name="registrar" type="registrar_info_t">
							<xsd:annotation>
								<xsd:documentation>Данные о регистраторе</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="issuer" type="issuer_info_t" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Информация об эмитенте</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="account_ident" type="account_ident_t" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Зарегистрированное лицо, по счету которого предоставляется отчет</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="base_doc" type="base_doc_t">
							<xsd:annotation>
								<xsd:documentation>Документ-основание , на который выполнен отказ</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="rejected_operation">
							<xsd:annotation>
								<xsd:documentation>Причина отказа и описание операции</xsd:documentation>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="reject_reason" type="xsd:string" maxOccurs="unbounded">
										<xsd:annotation>
											<xsd:documentation>Причина отказа </xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="recommendation" type="xsd:string" maxOccurs="unbounded">
										<xsd:annotation>
											<xsd:documentation>Действия по устранению причины отказа</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="operation" type="operation_t" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>Описание операции</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="make_datetime" type="xsd:dateTime">
							<xsd:annotation>
								<xsd:documentation>Дата/ время составления отчета</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="add_info" type="any_t" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Дополнительная информация</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="REQUEST_FOR_REGISTER_SHAREHOLDERS">
		<xsd:annotation>
			<xsd:documentation>Запрос (требование) на раскрытие информации номинальному держателю</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="doc_t">
					<xsd:sequence>
						<xsd:element name="registrar" type="registrar_info_t">
							<xsd:annotation>
								<xsd:documentation>Данные регистратора</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="issuer" type="issuer_info_t">
							<xsd:annotation>
								<xsd:documentation>Информация об эмитенте</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="account_ident" type="account_ident_t">
							<xsd:annotation>
								<xsd:documentation>Зарегистрированное лицо (НД), по счету которого запрашивается информация</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="corporate_action_code" type="corporate_action_event_et">
							<xsd:annotation>
								<xsd:documentation>Вид корпоративного действия</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="purpose_of_request" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Цель предоставления списка </xsd:documentation>
								<!-- ===================================================
Дополнительное текстовое описание корпоративного события
=================================================== -->
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="reason_indicator" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Основание запроса</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="information_indicator" type="information_indicator_et">
							<xsd:annotation>
								<xsd:documentation>Тип предоставляемой информации (тип списка)</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="record_date" type="xsd:date">
							<xsd:annotation>
								<xsd:documentation>Дата, на конец операционного дня которой запрашивается информация</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="certification_deadline_date" type="xsd:date">
							<xsd:annotation>
								<xsd:documentation>Дата, до которой должен быть предоставлен список</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="security_balance" type="security_balance_t" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Баланс по ценной бумаге </xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="make_datetime" type="xsd:dateTime">
							<xsd:annotation>
								<xsd:documentation>Дата/ время составления запроса</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="add_info" type="any_t" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Дополнительная информация</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="REGISTER_OF_SHAREHOLDERS">
		<xsd:annotation>
			<xsd:documentation>Список лиц, имеющих право</xsd:documentation>
			<!-- =========================================================================================== -->
			<!-- =========================================================================================== -->
		</xsd:annotation>
		<!-- =========================================================================================== -->
		<!-- =========================================================================================== -->
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="out_doc_t">
					<xsd:sequence>
						<xsd:element name="issuer" type="issuer_info_t">
							<xsd:annotation>
								<xsd:documentation>Информация об эмитенте</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="account_ident" type="account_ident_t">
							<xsd:annotation>
								<xsd:documentation>Зарегистрированное лицо (НД), по счету которого предоставляется информация</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="corporate_action_code" type="corporate_action_event_et">
							<xsd:annotation>
								<xsd:documentation>Вид корпоративного действия</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="record_date" type="xsd:date">
							<xsd:annotation>
								<xsd:documentation>Дата, на конец операционного дня которой составлены данные</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="message_function" type="message_function_et">
							<xsd:annotation>
								<xsd:documentation>Назначение предоставляемой информации (первичное раскрытие, дораскрытие) </xsd:documentation>
								<!--=================================================================================
								первичный список - NEWM, дораскрытие - AMND
								====================================================================================-->
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="previously_provided_list" type="doc_link_t" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Ссылка на ранее предоставленный список </xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="information_indicator" type="information_indicator_et">
							<xsd:annotation>
								<xsd:documentation>Тип предоставляемой информации (тип списка)</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="register_list" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Список владельцев </xsd:documentation>
								<!--==================================================================================
								блок повторяющийся  при дораскрытии ранее предоставленного списка
								=====================================================================================-->
							</xsd:annotation>
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="shareholder_id" type="id_t" minOccurs="0" maxOccurs="unbounded">
										<xsd:annotation>
											<xsd:documentation>Индивидуальный код указанного в блоке shareholder лица,  присвоенный номинальным держателем, предоставившим ранее список.</xsd:documentation>
											<!--===============================================================================
											Используется, как идентификатор shareholder при дораскрытии нераскрытого остатка на счете номинального держателя в ранее предоставленном списке. Заполняется только в случае дораскрытия списка. Повторяющийся элемент. 
											=================================================================================-->
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="shareholder" maxOccurs="unbounded">
										<xsd:annotation>
											<xsd:documentation>Блок информации о владельце ценных бумаг </xsd:documentation>
										</xsd:annotation>
										<xsd:complexType>
											<xsd:sequence>
												<xsd:element name="shareholder_id" type="id_t">
													<xsd:annotation>
														<xsd:documentation>Индивидуальный код указанного в блоке shareholder лица,  присвоенный номинальным держателем, предоставляющим данные</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="shareholder_info">
													<xsd:annotation>
														<xsd:documentation>Анкетные данные зарегистрированного лица</xsd:documentation>
													</xsd:annotation>
													<xsd:complexType>
														<xsd:sequence>
															<xsd:element name="shareholder_form" type="content_form_of_shareholders_t">
																<xsd:annotation>
																	<xsd:documentation>Анкета зарегистрированного лица</xsd:documentation>
																</xsd:annotation>
															</xsd:element>
															<xsd:element name="coowner" minOccurs="0" maxOccurs="unbounded">
																<xsd:annotation>
																	<xsd:documentation>Совладельцы счета общей собственности (если ЗЛ - счет ОС)</xsd:documentation>
																</xsd:annotation>
																<xsd:complexType>
																	<xsd:sequence>
																		<xsd:element name="coowner_form" type="content_form_of_coowner_t">
																			<xsd:annotation>
																				<xsd:documentation>Анкета совладельца</xsd:documentation>
																			</xsd:annotation>
																		</xsd:element>
																		<xsd:element name="quota" type="coowner_quota_t" minOccurs="0">
																			<xsd:annotation>
																				<xsd:documentation>Доля совладельца (для счетов ОС с долевой собственностью) </xsd:documentation>
																			</xsd:annotation>
																		</xsd:element>
																	</xsd:sequence>
																</xsd:complexType>
															</xsd:element>
															<xsd:element name="lei" type="xsd:string" minOccurs="0">
																<xsd:annotation>
																	<xsd:documentation>Код LEI конечного депозитария</xsd:documentation>
																</xsd:annotation>
															</xsd:element>
														</xsd:sequence>
													</xsd:complexType>
												</xsd:element>
												<xsd:element name="security_balance" type="security_balance_t" maxOccurs="unbounded">
													<xsd:annotation>
														<xsd:documentation>Блок информации об остатках ЦБ на счете</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
											</xsd:sequence>
										</xsd:complexType>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="add_info" type="any_t" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Дополнительная информация</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="REQUEST_FOR_RECONCILIATION">
		<xsd:annotation>
			<xsd:documentation>Запрос на сверку с трансфер-агентом (Журнал принятых/отправленных документов)</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="doc_t">
					<xsd:sequence>
						<xsd:element name="registrar" type="registrar_info_t">
							<xsd:annotation>
								<xsd:documentation>Данные регистратора</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="reg_subdiv" type="id_name_t">
							<xsd:annotation>
								<xsd:documentation>Подразделение регистратора - инициатор запроса сверки (ЦО или филиал)</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="transfer_agent" type="transfer_agent_info_t">
							<xsd:annotation>
								<xsd:documentation>Информация о трансфер-агенте, для которого сформирован запрос сверки</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="beg_date" type="xsd:date" nillable="true">
							<xsd:annotation>
								<xsd:documentation>Дата начала периода сверки</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="end_date" type="xsd:date" nillable="true">
							<xsd:annotation>
								<xsd:documentation>Дата конца периода сверки</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="doc_list" type="doc_info_t" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Список документов, принятых регистратором от трансфер-агента</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="total_docs" type="xsd:nonNegativeInteger">
							<xsd:annotation>
								<xsd:documentation>Количество документов в запросе на сверку</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="make_datetime" type="xsd:dateTime">
							<xsd:annotation>
								<xsd:documentation>Дата/ время составления запроса</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="add_info" type="any_t" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Дополнительная информация</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="STATEMENT_OF_RECONCILIATION">
		<xsd:annotation>
			<xsd:documentation>Акт сверки от трансфер-агента </xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="out_doc_t">
					<xsd:sequence>
						<xsd:element name="transfer_agent" type="transfer_agent_info_t">
							<xsd:annotation>
								<xsd:documentation>Информация о трансфер-агенте </xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="reg_subdiv" type="id_name_t">
							<xsd:annotation>
								<xsd:documentation>Подразделение регистратора, для которого сформирован акт сверки (ЦО или филиал)</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="beg_date" type="xsd:date" nillable="true">
							<xsd:annotation>
								<xsd:documentation>Дата начала периода сверки</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="end_date" type="xsd:date" nillable="true">
							<xsd:annotation>
								<xsd:documentation>Дата конца периода сверки</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="doc_list" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Список документов, принятых регистратором от трансфер-агента (входящие документы регистратора)</xsd:documentation>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="doc_info" type="doc_info_t">
										<xsd:annotation>
											<xsd:documentation>Информация о документе</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="reconciliation_info" type="reconciliation_info_t">
										<xsd:annotation>
											<xsd:documentation>Результат сверки</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="total_result" type="result_reconciliation_total_et">
							<xsd:annotation>
								<xsd:documentation>Итоговый результата сверки</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="total_docs" type="xsd:nonNegativeInteger">
							<xsd:annotation>
								<xsd:documentation>Количество документов в акте сверки</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="make_datetime" type="xsd:dateTime">
							<xsd:annotation>
								<xsd:documentation>Дата/ время создания акта (выполнения сверки)</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="add_info" type="any_t" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Дополнительная информация</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="INSTRUCTION_TO_DOC_RECALL">
		<xsd:annotation>
			<xsd:documentation>Распоряжение на отзыв документа</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="doc_t">
					<xsd:sequence>
						<xsd:element name="doc_link" type="doc_link2_t">
							<xsd:annotation>
								<xsd:documentation>Документ, который отзывается</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="reason" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Основание отзыва документа</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="scan_info" type="scan_info_t" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Скан образ бумажного оригинала документа (возможен у документа ТА)</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="add_info" type="any_t" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Дополнительная информация</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="REQUEST_OF_NOTARY">
		<xsd:annotation>
			<xsd:documentation>Запрос нотариуса</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="doc_t">
					<xsd:sequence>
						<xsd:element name="issuer" type="issuer_info_t">
							<xsd:annotation>
								<xsd:documentation>Информация об эмитенте</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="report_info">
							<xsd:annotation>
								<xsd:documentation>Данные запроса</xsd:documentation>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="account_ident" type="account_ident_t">
										<xsd:annotation>
											<xsd:documentation>Зарегистрированное лицо, по счету которого сформирован запрос нотариуса</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="authorised_person" type="authorised_person_info_t" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>Уполномоченный представитель ЗЛ (обязателен для юридических лиц)</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="date" type="date_or_datetime_t" nillable="true">
										<xsd:annotation>
											<xsd:documentation>Дата, на которую выполняется запрос нотариуса												</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="report_mode" type="report_mode_et">
							<xsd:annotation>
								<xsd:documentation>Форма предоставления отчета (бумажный оригинал, электронный документ)</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:sequence minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Если форма предоставления - бумажный оригинал</xsd:documentation>
							</xsd:annotation>
							<xsd:element name="e_copy" type="yes_no_t">
								<xsd:annotation>
									<xsd:documentation>Признак передачи электронной копии документа</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="report_delivery">
								<xsd:annotation>
									<xsd:documentation>Данные о выдаче ответного исходящего документа </xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="delivery_place" type="delivery_place_et" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation>Место выдачи - трансфер-агент или регистратор (заполнять для запросов от трансфер-агента)</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="delivery_method" type="delivery_method_et">
											<xsd:annotation>
												<xsd:documentation>Способ выдачи </xsd:documentation>
											</xsd:annotation>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
						<xsd:element name="comment" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Комментарий</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="scan_info" type="scan_info_t" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Скан образ бумажного оригинала документа (возможен у документа ТА)</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="appendix" type="doc_app_t" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Список приложений к документу (возможен у документа ТА)</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="add_info" type="any_t" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Дополнительная информация</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="FREE_FORMAT_MESSAGE">
		<xsd:annotation>
			<xsd:documentation>Сообщение-письмо в свободном формате</xsd:documentation>
			<!-- =========================================================================================== -->
			<!-- =========================================================================================== -->
		</xsd:annotation>
		<!-- =========================================================================================== -->
		<!-- =========================================================================================== -->
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="doc_t">
					<xsd:sequence>
						<xsd:element name="doc_link" type="doc_link_t" nillable="false" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Ссылка на связанный документ</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="registrar" type="registrar_info_t" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Данные о регистраторе</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="issuer" type="issuer_info_t" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Информация об эмитенте</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="text" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Текст сообщения - письма</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="scan_info" type="scan_info_t" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Скан образ бумажного оригинала документа (возможен у документа ТА)</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="appendix" type="doc_app_t" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Список приложений к документу (возможен у документа ТА)</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="add_info" type="any_t" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Дополнительная информация</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<!--......................................................................................................................................................................................-->
	<xsd:element name="NOTICE_OF_REPORT">
		<xsd:annotation>
			<xsd:documentation>Уведомление о сформированном отчете (формируется регистратором в ответ на запрос отчета во внешнем формате, файл самого отчета передается вместе с ним)</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="out_doc_t">
					<xsd:sequence>
						<xsd:element name="report_type">
							<xsd:annotation>
								<xsd:documentation>Тип отчета </xsd:documentation>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="id_report" type="id_t">
										<xsd:annotation>
											<xsd:documentation>Код отчета по внутренней кодировке регистратора</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="narrative" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>Наименование отчета</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="report_mode" type="report_mode_et">
							<xsd:annotation>
								<xsd:documentation>Форма предоставления отчета абоненту (бумажный оригинал, электронный документ)</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="e_copy" type="yes_no_t" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Признак передачи электронной копии документа (если форма предоставления - бумажный оригинал)</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="report_delivery" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Данные о выдаче ответного исходящего документа </xsd:documentation>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="delivery_place" type="delivery_place_et" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>Место выдачи - трансфер-агент или регистратор (заполнять для запросов от трансфер-агента)</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="delivery_method" type="delivery_method_et">
										<xsd:annotation>
											<xsd:documentation>Способ выдачи </xsd:documentation>
										</xsd:annotation>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="format" type="format_report_t" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Формат, в котором сформирован отчет</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="make_datetime" type="xsd:dateTime">
							<xsd:annotation>
								<xsd:documentation>Дата/ время формирования отчета</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="add_info" type="any_t" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Дополнительная информация</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="NOTICE_OF_DOC_RECEPTION">
		<xsd:annotation>
			<xsd:documentation>Уведомление о получении документа</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="out_doc_t">
					<xsd:sequence>
						<xsd:element name="reg_datetime" type="xsd:dateTime">
							<xsd:annotation>
								<xsd:documentation>Дата/ время регистрации документа</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="add_info" type="any_t" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Дополнительная информация</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="NOTICE_OF_OUTDOC_GIVE">
		<xsd:annotation>
			<xsd:documentation>Сведения о выдаче исходящего документа</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="doc_t">
					<xsd:sequence>
						<xsd:element name="outdoc_link" type="doc_link_t">
							<xsd:annotation>
								<xsd:documentation>Исходящий документ </xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="delivery_state" type="yes_no_t">
							<xsd:annotation>
								<xsd:documentation>Отметка о выдаче документа</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="delivery_date" type="date_or_datetime_t" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Дата выдачи</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="doc_delivery" type="delivery_doc_t" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Данные о выдаче исходящего документа </xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="add_info" type="any_t" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Дополнительная информация</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<!---->
	<!--........... XML - ТИПЫ ДАННЫХ ........................................................................................................................................-->
	<!--........... Простые типы данных .......................................................................................................................................-->
	<xsd:simpleType name="decimal_t">
		<xsd:annotation>
			<xsd:documentation>десятичное число</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:decimal"/>
	</xsd:simpleType>
	<xsd:simpleType name="yes_no_t">
		<xsd:annotation>
			<xsd:documentation>признак "Да" (Yes)/ "Нет" (No)</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Yes">
				<xsd:annotation>
					<xsd:documentation>Yes - Да</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="No">
				<xsd:annotation>
					<xsd:documentation>No - Нет</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="uuid_t">
		<xsd:annotation>
			<xsd:documentation>Тип: глобальный уникальный идентификатор
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:whiteSpace value="collapse"/>
			<xsd:pattern value="[a-f0-9]{8}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{12}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!--........... Составные типы данных ....................................................................................................................................-->
	<xsd:complexType name="account_ident_t">
		<xsd:annotation>
			<xsd:documentation>Идентификатор лицевого счета ЗЛ в реестре</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="account_number" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Номер лицевого счета </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="account_type" type="account_type_et">
				<xsd:annotation>
					<xsd:documentation>Вид счета</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="person_info" type="person_info_t">
				<xsd:annotation>
					<xsd:documentation>Информация о зарегистрированном лице (наименование и данные документа)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="address_legal_t">
		<xsd:annotation>
			<xsd:documentation>Юридический адрес</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:choice>
				<xsd:element name="address_kladr" type="address_struct_t">
					<xsd:annotation>
						<xsd:documentation>адрес по КЛАДР</xsd:documentation>
						<!-- ===================================================
По согласованию сторон используется один из вариантов
=================================================== -->
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="address_nostruct" type="address_nostruct_t">
					<xsd:annotation>
						<xsd:documentation>неструктурированный адрес</xsd:documentation>
						<!-- ===================================================
По согласованию сторон используется один из вариантов
=================================================== -->
					</xsd:annotation>
				</xsd:element>
			</xsd:choice>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="address_post_t">
		<xsd:annotation>
			<xsd:documentation>Почтовый адрес</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:choice>
				<xsd:element name="address_kladr" type="address_struct_t">
					<xsd:annotation>
						<xsd:documentation>адрес по КЛАДР</xsd:documentation>
						<!-- ===================================================
По согласованию сторон используется один из вариантов
=================================================== -->
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="address_nostruct" type="address_nostruct_t">
					<xsd:annotation>
						<xsd:documentation>неструктурированный адрес</xsd:documentation>
						<!-- ===================================================
По согласованию сторон используется один из вариантов
=================================================== -->
					</xsd:annotation>
				</xsd:element>
			</xsd:choice>
			<xsd:element name="another" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Получатель корреспонденции - иное лицо</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="address_struct_t">
		<xsd:annotation>
			<xsd:documentation>Структурированный адрес</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="country" type="country_et" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Страна из Общероссийского классификатора стран мира ОК (МК (ИСО 3166) 004-97) 025-2001</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="index" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Почтовый индекс</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:choice minOccurs="0">
				<xsd:element name="region_number" type="region_et" nillable="true">
					<xsd:annotation>
						<xsd:documentation>Код региона по справочнику регионов России</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:sequence>
					<xsd:element name="region_type" type="xsd:string">
						<xsd:annotation>
							<xsd:documentation>Тип региона</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="region" type="xsd:string">
						<xsd:annotation>
							<xsd:documentation>Наименование региона</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:choice>
			<xsd:sequence minOccurs="0">
				<xsd:element name="zonet_type" type="xsd:string">
					<xsd:annotation>
						<xsd:documentation>Тип района</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="zone" type="xsd:string">
					<xsd:annotation>
						<xsd:documentation>Район</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
			<xsd:sequence minOccurs="0">
				<xsd:element name="city_type" type="xsd:string">
					<xsd:annotation>
						<xsd:documentation>Тип города</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="city" type="xsd:string">
					<xsd:annotation>
						<xsd:documentation>Город</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
			<xsd:sequence minOccurs="0">
				<xsd:element name="place_type" type="xsd:string">
					<xsd:annotation>
						<xsd:documentation>Тип населенного пункта</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="place" type="xsd:string">
					<xsd:annotation>
						<xsd:documentation>Населенный пункт</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
			<xsd:sequence minOccurs="0">
				<xsd:element name="street_type" type="xsd:string">
					<xsd:annotation>
						<xsd:documentation>Тип улицы</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="street" type="xsd:string">
					<xsd:annotation>
						<xsd:documentation>Улица</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
			<xsd:element name="house" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Номер дома/ домовладения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="building" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Корпус/строение</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="flat" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Квартира/офис</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="address_nostruct_t">
		<xsd:annotation>
			<xsd:documentation>Неструктурированный адрес</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="country" type="country_et" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>код страны</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="index" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>почтовый индекс</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="address" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>адрес</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="any_t" mixed="true">
		<xsd:annotation>
			<xsd:documentation>неконтролируемый блок для расширений</xsd:documentation>
			<!-- ===================================================
Элемент может содержать произвольную информацию: текст, другие элементы, двоичную информацию (графику, цифровые подписи, звук, …) и т.д.. 
=================================================== -->
		</xsd:annotation>
		<xsd:sequence>
			<xsd:any processContents="lax" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Неконтролируемый блок для расширений. Элемент может содержать произвольную информацию: текст, другие элементы, двоичную информацию (графику, цифровые подписи, звук, …) и т.д.. </xsd:documentation>
				</xsd:annotation>
			</xsd:any>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="authorised_person_ident_t">
		<xsd:annotation>
			<xsd:documentation>Индентификатор уполномоченного представителя</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="authorised_person_id" type="xsd:nonNegativeInteger" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Номер представителя уникальный</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="authorised_person" type="authorised_person_info_t">
				<xsd:annotation>
					<xsd:documentation>Уполномоченный представитель / должностное лицо</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="authorised_person_info_t">
		<xsd:annotation>
			<xsd:documentation>Характеристика уполномоченного представителя</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="authorised_person_type" type="authorised_person_type_et">
				<xsd:annotation>
					<xsd:documentation>тип уполномоченного представителя / должностного лица </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="authorised_person_position" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Должность (для должностных лиц)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="authorised_person_info" type="person_info2_t">
				<xsd:annotation>
					<xsd:documentation>основные данные уполномоченного представителя (наименование и документ)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="bases_of_powers" type="bases_of_powers_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>основания для полномочий (устав, доверенность)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="bank_t">
		<xsd:annotation>
			<xsd:documentation>Банковские реквизиты</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:choice>
				<xsd:element name="bank_struct" type="bank_prop_rub_t">
					<xsd:annotation>
						<xsd:documentation>структурированные банковские реквизиты (для оплаты в рублях)</xsd:documentation>
						<!-- ===================================================
По согласованию сторон используется один из вариантов
=================================================== -->
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="bank_nostruct" type="xsd:string">
					<xsd:annotation>
						<xsd:documentation>неструктурированные банковские реквизиты</xsd:documentation>
						<!-- ===================================================
По согласованию сторон используется один из вариантов
=================================================== -->
					</xsd:annotation>
				</xsd:element>
			</xsd:choice>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="bank_prop_rub_t">
		<xsd:annotation>
			<xsd:documentation>Банковские реквизиты лица для расчетов в рублях</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="cash_rub_dtls" type="cash_rub_dtls_t">
				<xsd:annotation>
					<xsd:documentation>информация о банковских реквизитах в рублях</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="pay_add_info" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дополнительные реквизиты платежа (данные, которые необходимы в поле «Назначение платежа»)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:sequence minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Наименование и ИНН плательщика/ получателя, если им является иное лицо</xsd:documentation>
				</xsd:annotation>
				<xsd:element name="pay_name" type="xsd:string">
					<xsd:annotation>
						<xsd:documentation>Наименование плательщика/ получателя </xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="inn" type="xsd:string" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>код ИНН  плательщика/ получателя</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="base_doc_t">
		<xsd:annotation>
			<xsd:documentation>Описание документа-основания операции в реестре</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="doc_info" type="doc_link_t">
				<xsd:annotation>
					<xsd:documentation>Номер, дата документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="doc_uid" type="uuid_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Уникальный идентификатор документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="doc_name" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Наименование документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="based_info_t">
		<xsd:annotation>
			<xsd:documentation>основание - список договоров</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="contract_type" type="contract_type_t">
				<xsd:annotation>
					<xsd:documentation>Вид договора/ иного основания</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="doc_num" type="xsd:string" nillable="true">
				<xsd:annotation>
					<xsd:documentation>Номер договора/ иного основания</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="doc_date" type="xsd:date" nillable="true">
				<xsd:annotation>
					<xsd:documentation>Дата договора/ иного основания</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="bases_of_powers_t">
		<xsd:annotation>
			<xsd:documentation>Основание полномочий уполномоченного представителя</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="bases_of_powers_document" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>документ (устав, доверенность,
акт, удостоверение), подтверждающий полномочия</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="bases_of_powers_number" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>номер документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="bases_of_powers_date" type="xsd:date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>дата документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="bases_of_powers_end_date" type="xsd:date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>дата окончания действия полномочий</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="legitim" type="yes_no_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Признак легитимности ЕИО после окончания его полномочий </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="block_type_t">
		<xsd:annotation>
			<xsd:documentation>Вид обременения ЦБ</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="block_type_code" type="block_type_et">
				<xsd:annotation>
					<xsd:documentation>Код вида обременения</xsd:documentation>
					<!-- ===================================================
Код вида обременения, должен принимать значения из фиксированного перечня. 
=================================================== -->
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="narrative" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Текстовое описание вида обременения</xsd:documentation>
					<!-- ===================================================
Заполняется обязательно, только в случае, если значением кода вида обременения указано OTHR
=================================================== -->
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="block_info_t">
		<xsd:annotation>
			<xsd:documentation>Описание блокирования ЦБ</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="id_block" type="xsd:nonNegativeInteger" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Уникальный номер обременения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="block_date" type="xsd:date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дата установки блокирования ЦБ</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="block_type" type="block_type_t">
				<xsd:annotation>
					<xsd:documentation>Вид обременения (BLOK- блокирование ЦБ, BLOP- блокирование заложенных, BLON- блокирование неоплаченных)</xsd:documentation>
					<!-- ===================================================
 Не подлежит заполнению, т.к. признак наличия обременения может быть No
=================================================== -->
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="repeat_block" type="yes_no_t">
				<xsd:annotation>
					<xsd:documentation>Признак повторного блокирования ЦБ (блокирование блокированных ЦБ)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="block_reason" type="block_reason_et" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Основание для блокирования ЦБ</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="security" type="security_info_t">
				<xsd:annotation>
					<xsd:documentation>Ценная бумага</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="quantity" type="units_or_fraction_t">
				<xsd:annotation>
					<xsd:documentation>Количество ЦБ, на которое установлено блокирование</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="condition_block" type="condition_block_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Условия блокирования ЦБ</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="comment" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Комментарий к описанию блокирования</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="based_info" type="based_info_t" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Основание для проведения операции</xsd:documentation>
					<!-- ===================================================
(повторяющийся элемент)
=================================================== -->
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="blockoper_type_t">
		<xsd:annotation>
			<xsd:documentation>Вид блокирования операций по счету</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="blockoper_type_code" type="blockoper_type_et">
				<xsd:annotation>
					<xsd:documentation>Код вида блокирования операций</xsd:documentation>
					<!-- ===================================================
Код вида обременения, должен принимать значения из фиксированного перечня. 
=================================================== -->
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="narrative" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Текстовое описание вида блокирования операций</xsd:documentation>
					<!-- ===================================================
Заполняется обязательно, только в случае, если значением кода вида обременения указано OTHR
=================================================== -->
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="blockoper_info_t">
		<xsd:annotation>
			<xsd:documentation>Описание блокирования операций с ценными бумагами</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="id_blockoper" type="xsd:nonNegativeInteger" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Уникальный номер блокирования операций</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="blockoper_date" type="xsd:date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дата установки блокирования операций</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="blockoper_type" type="blockoper_type_t">
				<xsd:annotation>
					<xsd:documentation>Вид блокирования операций (всех операций, расходных, приходно-расходных, всех операций с ЦБ, расходных и залогов)</xsd:documentation>
					<!-- ===================================================
 Не подлежит заполнению, т.к. признак наличия обременения может быть No
=================================================== -->
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="block_reason" type="block_reason_et" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Основание для блокирования</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="comment" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Комментарий</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="cash_rub_dtls_t">
		<xsd:annotation>
			<xsd:documentation>информация о банковских реквизитах в рублях</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:choice>
				<xsd:element name="account" type="xsd:string">
					<xsd:annotation>
						<xsd:documentation>лицевой/расчетный счета лица</xsd:documentation>
						<!-- ===================================================
	Cч № (номер лицевого счета стороны (плательщика/ получателя) в банке
	=================================================== -->
					</xsd:annotation>
				</xsd:element>
				<xsd:sequence>
					<xsd:annotation>
						<xsd:documentation>Счет открыт в филиале (отделении) банка</xsd:documentation>
					</xsd:annotation>
					<xsd:element name="account_person" type="xsd:string">
						<xsd:annotation>
							<xsd:documentation>лицевой/расчетный счета лица</xsd:documentation>
							<!-- ===================================================
	Cч № (номер лицевого счета стороны (плательщика/ получателя) в банке
	=================================================== -->
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="bank_branch_name" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>наименование филиала (отделения) банка</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="account_branch" type="xsd:string">
						<xsd:annotation>
							<xsd:documentation>расчетный счет филиала (отделения) банка</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:choice>
			<xsd:element name="bank_name" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>наименование банка</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="bank_city" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>город банка</xsd:documentation>
					<!-- ===================================================
Город банка стороны (плательщика/ получателя)
=================================================== -->
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ruic" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>БИК банка</xsd:documentation>
					<!-- ===================================================
БИК банка стороны (плательщика/ получателя) - банковский идентификационный код, присваиваемый Банком России кредитным организациям
=================================================== -->
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="bank_corr" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>корреспондентский счет банка</xsd:documentation>
					<!-- ===================================================
					Cч №  (номер корреспондентского счета банка стороны (плательщика/ получателя) в одном из расчетных учреждений (подразделений) Банка России
				=================================================== -->
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="bank_inn" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>ИНН банка</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="condition_block_t">
		<xsd:annotation>
			<xsd:documentation>Условия блокирования ЦБ</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="block_divid" type="yes_no_t">
				<xsd:annotation>
					<xsd:documentation>Блокируются права на получение дивидендов</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="block_vote" type="yes_no_t">
				<xsd:annotation>
					<xsd:documentation>Блокируются голоса</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="info_add" type="xsd:string" nillable="false" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дополнительные условия </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="date_end" type="xsd:date" nillable="false" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Планируемая дата окончания блокирования</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="condition_pledge_t">
		<xsd:annotation>
			<xsd:documentation>Условия залога ценных бумаг</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="no_repledge" type="yes_no_t" nillable="false">
				<xsd:annotation>
					<xsd:documentation>поледующий залог запрещается</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="transfer" type="yes_no_t" nillable="false">
				<xsd:annotation>
					<xsd:documentation>передача заложенных допускается без согласия залогодержателя </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="concession" type="yes_no_t" nillable="false">
				<xsd:annotation>
					<xsd:documentation>Уступка прав по договору залога без согласия залогодержателя запрещается </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:sequence>
				<xsd:element name="convert" type="condition_type_et">
					<xsd:annotation>
						<xsd:documentation>Залог распространяется на ЦБ после конвертации</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="quantity_convert" type="units_or_fraction_t" nillable="false" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>на определенное количество ЦБ (при convert=part)</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
			<xsd:sequence>
				<xsd:element name="addition" type="yes_no_t">
					<xsd:annotation>
						<xsd:documentation>Залог распространяется на дополнительно зачисляемые на счет залогодателя ЦБ</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:sequence minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>Заполняется в случае распространения залога на  дополнительно зачисляемые ЦБ (addition=Yes)</xsd:documentation>
					</xsd:annotation>
					<xsd:element name="security_classification" type="security_classification_et" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Вид ценной бумаги</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="security_category" type="security_category_et" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Категория ценной бумаги по предоставляемым правам</xsd:documentation>
							<!-- ===================================================
Заполняется только в случае, если вид ценной бумаги – SHAR –Акция
=================================================== -->
						</xsd:annotation>
					</xsd:element>
					<xsd:choice>
						<xsd:element name="quantity_addition" type="units_or_fraction_t" nillable="false">
							<xsd:annotation>
								<xsd:documentation>на определенное количество ЦБ</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:sequence>
							<xsd:annotation>
								<xsd:documentation>на количество ЦБ, пропорциональное всем или части заложенных ЦБ </xsd:documentation>
							</xsd:annotation>
							<xsd:element name="pro_all_quantity" type="yes_no_t">
								<xsd:annotation>
									<xsd:documentation>признак - пропорционально всем или части заложенных ЦБ</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="quantity_part" type="units_or_fraction_t" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Количество, составляющее часть заложенных ЦБ (pro_addition=part)</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
					</xsd:choice>
				</xsd:sequence>
			</xsd:sequence>
			<xsd:sequence>
				<xsd:element name="deleg_divid" type="condition_type_et">
					<xsd:annotation>
						<xsd:documentation>Получателем дохода является залогодержатель</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="quantity_divid" type="units_or_fraction_t" nillable="false" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>на определенное количество ЦБ (при deleg_divid=part)</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
			<xsd:sequence>
				<xsd:element name="deleg_vote" type="condition_type_et">
					<xsd:annotation>
						<xsd:documentation>Правом голоса обладает залогодержатель</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="quantity_vote" type="units_or_fraction_t" nillable="false" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>на определенное количество ЦБ (при deleg_vote=part)</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
			<xsd:sequence>
				<xsd:element name="bonds_provision" type="yes_no_t" nillable="false">
					<xsd:annotation>
						<xsd:documentation>Залог в обеспечение исполнения обязательств по облигациям </xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="bonds_info" type="xsd:string" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>Описание облигаций, в случае обеспечения залога (bonds_provision=Yes) </xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
			<xsd:sequence minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Обращение взыскания на заложенные ЦБ осуществляется во внесудебном порядке</xsd:documentation>
				</xsd:annotation>
				<xsd:element name="date" type="xsd:date" nillable="true">
					<xsd:annotation>
						<xsd:documentation>Дата взыскания на заложенные ЦБ </xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="term" type="xsd:string" nillable="true" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>Срок реализации заложенных ЦБ</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:sequence>
					<xsd:annotation>
						<xsd:documentation>Документы, предоставляемые залогодержателем реестродержателю при прекращении залога и обращении взыскания во внесудебном порядке</xsd:documentation>
					</xsd:annotation>
					<xsd:element name="doc_statement" type="yes_no_t">
						<xsd:annotation>
							<xsd:documentation>Выписка из реестра сделок организатора торгов, подтверждающая заключение сделки</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="doc_commission" type="yes_no_t">
						<xsd:annotation>
							<xsd:documentation>Договор купли-продажи ЦБ, заключенный комиссионером, и договор комиссии между залогодержателем и комиссионером</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="doc_protocol" type="yes_no_t">
						<xsd:annotation>
							<xsd:documentation>Протокол несостоявшихся повторных торгов</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="doc_copy" type="yes_no_t">
						<xsd:annotation>
							<xsd:documentation>Копии уведомлений залогодержателем залогодателя (о начале обращения взыскания и (или) о приобретении заложенных ЦБ)</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="doc_sale" type="yes_no_t">
						<xsd:annotation>
							<xsd:documentation>Договор купли-продажи заложенных ЦБ, заключенный по результатам торгов</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:sequence>
			<xsd:sequence minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>ЦБ передаются в залог суду или органу, в производстве которого находится уголовное дело</xsd:documentation>
				</xsd:annotation>
				<xsd:element name="case_num" type="xsd:string">
					<xsd:annotation>
						<xsd:documentation>Номер уголовного дела</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="person" type="xsd:string" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>ФИО лица, за которое вносится залог</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
			<xsd:element name="info_add" type="xsd:string" nillable="false" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>дополнительные условия обременения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="contacts_t">
		<xsd:annotation>
			<xsd:documentation>Контактная информация лица</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="phone_or_fax" type="phone_t" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>номер телефона</xsd:documentation>
					<!-- ===================================================
(повторяющийся элемент)
=================================================== -->
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="e_mail" type="xsd:string" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>адрес электронной почты</xsd:documentation>
					<!-- ===================================================
(повторяющийся элемент)
=================================================== -->
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="content_form_of_authorised_t">
		<xsd:annotation>
			<xsd:documentation>Анкета уполномоченного представителя</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="authorised_info">
				<xsd:annotation>
					<xsd:documentation>Основные анкетные данные лица</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:choice>
						<xsd:element name="juridical" type="form_juridical_t">
							<xsd:annotation>
								<xsd:documentation>Анкета юридического лица</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="individual" type="form_individual_t">
							<xsd:annotation>
								<xsd:documentation>Анкета физического лица</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:choice>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="contacts" type="contacts_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Контактная информация</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="letter_go_type" type="letter_go_type_et">
				<xsd:annotation>
					<xsd:documentation>Способ доставки корреспонденции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="comment" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Комментарий к анкете</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="content_form_of_coowner_t">
		<xsd:annotation>
			<xsd:documentation>Анкета совладельца</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="coowner_numb" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Номер совладельца по счету ОС</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="coowner_info">
				<xsd:annotation>
					<xsd:documentation>Основные анкетные данные совладельца</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:choice>
						<xsd:element name="juridical" type="form_juridical_t">
							<xsd:annotation>
								<xsd:documentation>Анкета юридического лица</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="individual" type="form_individual_t">
							<xsd:annotation>
								<xsd:documentation>Анкета физического лица</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:choice>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="contacts" type="contacts_t">
				<xsd:annotation>
					<xsd:documentation>Контактная информация</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="bank_details" type="bank_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Банковские реквизиты </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="tax_status" type="tax_category_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Категория налогоплательщика</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="distr_div_frm" type="distr_div_frm_et">
				<xsd:annotation>
					<xsd:documentation>Форма выплаты доходов по ценным бумагам</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="letter_go_type" type="letter_go_type_et">
				<xsd:annotation>
					<xsd:documentation>Способ доставки корреспонденции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="doc_by_post" type="yes_no_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Допустимо предоставление документов регистратору почтовым отправлением</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="auto_mfy" type="yes_no_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Согласие в случае непредоставления анкеты в течение года считать сведения обновленными</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="comment" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Комментарий к анкете</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="content_form_of_issuer_t">
		<xsd:annotation>
			<xsd:documentation>Анкета эмитента</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="issuer_info" type="form_juridical_t">
				<xsd:annotation>
					<xsd:documentation>основная информация о эмитенте</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="bank_details" type="bank_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Банковские реквизиты эмитента</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="contacts" type="contacts_t">
				<xsd:annotation>
					<xsd:documentation>контактная информация</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="authorized_capital" type="currency_and_amount_t">
				<xsd:annotation>
					<xsd:documentation>Размер уставного (складочного) капитала </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="letter_go_type" type="letter_go_type_et">
				<xsd:annotation>
					<xsd:documentation>Способ доставки корреспонденции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="code_fcsm" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Код эмитента ФСФР</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="content_form_of_shareholders_t">
		<xsd:annotation>
			<xsd:documentation>Анкета зарегистрированного лица</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="account_type" type="account_type_et">
				<xsd:annotation>
					<xsd:documentation>Вид счета</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="shareholder_info">
				<xsd:annotation>
					<xsd:documentation>Основные анкетные данные ЗЛ</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:choice>
						<xsd:element name="juridical" type="form_juridical_t">
							<xsd:annotation>
								<xsd:documentation>Анкета юридического лица</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="individual" type="form_individual_t">
							<xsd:annotation>
								<xsd:documentation>Анкета физического лица</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="coowners" type="form_coownership_t">
							<xsd:annotation>
								<xsd:documentation>Анкета счета общей собственности</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:choice>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="contacts" type="contacts_t">
				<xsd:annotation>
					<xsd:documentation>Контактная информация</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="bank_details" type="bank_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Банковские реквизиты </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="tax_status" type="tax_category_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Категория налогоплательщика</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="distr_div_frm" type="distr_div_frm_et">
				<xsd:annotation>
					<xsd:documentation>Форма выплаты доходов по ценным бумагам</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="letter_go_type" type="letter_go_type_et">
				<xsd:annotation>
					<xsd:documentation>Способ доставки корреспонденции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="doc_by_post" type="yes_no_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Допустимо предоставление документов регистратору почтовым отправлением</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="auto_mfy" type="yes_no_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Согласие в случае непредоставления анкеты в течение года считать сведения обновленными</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="comment" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Комментарий к анкете</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="contract_type_t">
		<xsd:annotation>
			<xsd:documentation>вид договора/ иного основания</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="contract_code" type="contract_et">
				<xsd:annotation>
					<xsd:documentation>код вида договора</xsd:documentation>
					<!-- ===================================================
Бзаполняется в соответствии со справочником кодов видов договоров/ иных оснований
=================================================== -->
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="narrative" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>текстовое описание наименование договора/ иного основания</xsd:documentation>
					<!-- ===================================================
Заполняется обязательно, только в случае, если значением кода вида договора/ иного основания указан код OTHR
=================================================== -->
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="coowner_quota_t" mixed="true">
		<xsd:annotation>
			<xsd:documentation>Доля совладельца</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="numerator" type="xsd:nonNegativeInteger">
				<xsd:annotation>
					<xsd:documentation>Числитель</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="denominator" type="xsd:positiveInteger">
				<xsd:annotation>
					<xsd:documentation>Знаменатель</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="coowner_info_t" block="extension" final="extension">
		<xsd:annotation>
			<xsd:documentation>Характеристика совладельца</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="id_coowner" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Уникальный номер совладельца по счету ОС</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:choice>
				<xsd:element name="juridical" type="juridical_info_t">
					<xsd:annotation>
						<xsd:documentation>Характеристика юридического лица</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="individual" type="individual_info_t">
					<xsd:annotation>
						<xsd:documentation>Характеристика  физического лица</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:choice>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="coowner_info_quota_t">
		<xsd:annotation>
			<xsd:documentation>Характеристика совладельца и его доля</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="coowner_ident" type="coowner_info_t">
				<xsd:annotation>
					<xsd:documentation>Идентификатор совладельца, доля которого изменяется</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="quota" type="coowner_quota_t">
				<xsd:annotation>
					<xsd:documentation>Доля совладельца </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="coownership_info_t">
		<xsd:annotation>
			<xsd:documentation>Характеристика счета общей собственности</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="coowners_name" nillable="false">
				<xsd:annotation>
					<xsd:documentation>полное наименование  счета общей собственности</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="coowners_list" type="coowner_info_t" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>перечень совладельцев счета общей собственности</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="currency_and_amount_t">
		<xsd:annotation>
			<xsd:documentation>валюта и сумма</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="ccy_code" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Код валюты - 3-х значные буквенные коды в соответствии с Общероссийским классификатором валют ОК (МК(ИСО 4217) 003-97) 014-2000 (ОКВ) </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="amount" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>сумма</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="date_or_datetime_t">
		<xsd:annotation>
			<xsd:documentation>дата или дата и время</xsd:documentation>
		</xsd:annotation>
		<xsd:choice>
			<xsd:element name="date" type="xsd:date">
				<xsd:annotation>
					<xsd:documentation>Дата</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="datetime" type="xsd:dateTime">
				<xsd:annotation>
					<xsd:documentation>Дата и время</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:choice>
	</xsd:complexType>
	<xsd:complexType name="date_or_period_t">
		<xsd:annotation>
			<xsd:documentation>дата или диапазон дат </xsd:documentation>
		</xsd:annotation>
		<xsd:choice minOccurs="0">
			<xsd:element name="date" type="date_or_datetime_t" nillable="true">
				<xsd:annotation>
					<xsd:documentation>Дата												</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:sequence>
				<xsd:element name="begin_date" type="xsd:date" nillable="true">
					<xsd:annotation>
						<xsd:documentation>Дата начала периода</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="end_date" type="xsd:date" nillable="true">
					<xsd:annotation>
						<xsd:documentation>Дата окончания периода
													</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
		</xsd:choice>
	</xsd:complexType>
	<xsd:complexType name="delivery_doc_t">
		<xsd:annotation>
			<xsd:documentation>Данные о выдаче исходящего документа</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="delivery_place" type="delivery_place_et" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Место выдачи - трансфер-агент или регистратор (заполнено для документов, сформированных по запросам от трансфер-агентов)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="delivery_method" type="delivery_method_et">
				<xsd:annotation>
					<xsd:documentation>Способ выдачи </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="receiver_name" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Получатель документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="receiver_address" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Почтовый адрес получателя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="doc_indiv_business_t">
		<xsd:annotation>
			<xsd:documentation>Документ индивидуального предпринимателя</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="indiv_business_type" type="indiv_business_type_et">
				<xsd:annotation>
					<xsd:documentation>тип индивидуальной деятельности (ИП, КФХ, нотариус)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:choice>
				<xsd:element name="ogrn_indiv" type="juridical_ogrn_t">
					<xsd:annotation>
						<xsd:documentation>информация о регистрации индивидуального предпринимателя или фермерского хозяйства, для indiv_business_type = INDV, FERM </xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="doc_notary">
					<xsd:annotation>
						<xsd:documentation>документы нотариуса, для indiv_business_type = NOTR </xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="licence" type="licence_t">
								<xsd:annotation>
									<xsd:documentation>лицензия на право нотариальной деятельности</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="order">
								<xsd:annotation>
									<xsd:documentation>приказ о назначении на должность</xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="order_num" type="xsd:string" nillable="true">
											<xsd:annotation>
												<xsd:documentation>номер приказа</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="order_date" type="xsd:date" nillable="true">
											<xsd:annotation>
												<xsd:documentation>дата приказа</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:choice>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="doc_t" mixed="false">
		<xsd:annotation>
			<xsd:documentation>Документ, который инициируется участником обмена</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="msg_t">
				<xsd:sequence>
					<xsd:element name="doc_num" type="xsd:string" nillable="true">
						<xsd:annotation>
							<xsd:documentation>Исходящий номер документа у отправителя							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="doc_date" type="date_or_datetime_t">
						<xsd:annotation>
							<xsd:documentation>Дата документа
										</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="sender" type="doc_sender_t">
						<xsd:annotation>
							<xsd:documentation>Отправитель документа</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="doc_TA" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Документ от трансфер-агента</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="transfer_agent" type="transfer_agent_info_t">
									<xsd:annotation>
										<xsd:documentation>Информация о трансфер-агенте</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="TA_doc_type" type="doc_type_t" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation>Тип документа</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="TA_doc_num" type="xsd:string">
									<xsd:annotation>
										<xsd:documentation>Входящий номер документа у трансфер-агента	</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="TA_doc_date" type="date_or_datetime_t">
									<xsd:annotation>
										<xsd:documentation>Дата регистрации документа у трансфер-агента
										</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="outdoc_delivery" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation>Данные о выдаче ответного исходящего документа </xsd:documentation>
									</xsd:annotation>
									<xsd:complexType>
										<xsd:sequence>
											<xsd:element name="report_mode" type="report_mode_et">
												<xsd:annotation>
													<xsd:documentation>Форма предоставления отчета (бумажный оригинал, электронный документ)</xsd:documentation>
												</xsd:annotation>
											</xsd:element>
											<xsd:sequence minOccurs="0">
												<xsd:annotation>
													<xsd:documentation>Если форма предоставления - бумажный оригинал</xsd:documentation>
												</xsd:annotation>
												<xsd:element name="e_copy" type="yes_no_t">
													<xsd:annotation>
														<xsd:documentation>Признак передачи электронной копии документа</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="report_delivery">
													<xsd:annotation>
														<xsd:documentation>Данные о выдаче ответного исходящего документа </xsd:documentation>
													</xsd:annotation>
													<xsd:complexType>
														<xsd:sequence>
															<xsd:element name="delivery_place" type="delivery_place_et">
																<xsd:annotation>
																	<xsd:documentation>Место выдачи - трансфер-агент или регистратор </xsd:documentation>
																</xsd:annotation>
															</xsd:element>
															<xsd:element name="delivery_method" type="delivery_method_et">
																<xsd:annotation>
																	<xsd:documentation>Способ выдачи </xsd:documentation>
																</xsd:annotation>
															</xsd:element>
														</xsd:sequence>
													</xsd:complexType>
												</xsd:element>
											</xsd:sequence>
										</xsd:sequence>
									</xsd:complexType>
								</xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="doc_app_t">
		<xsd:annotation>
			<xsd:documentation>Документ-приложение</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="app_type" type="doc_type_t">
				<xsd:annotation>
					<xsd:documentation>Тип документа- приложения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="app_num" type="xsd:string" nillable="true">
				<xsd:annotation>
					<xsd:documentation>Номер документа у отправителя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="app_date" type="date_or_datetime_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дата документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="scan_info" type="scan_info_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Скан образ бумажного оригинала приложения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="doc_info_t">
		<xsd:annotation>
			<xsd:documentation>Описание входящего документа</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="doc_link" type="doc_link_t">
				<xsd:annotation>
					<xsd:documentation>Номер, дата документа у регистратора</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="doc_TA">
				<xsd:annotation>
					<xsd:documentation>Номер, дата документа у трансфер-агента</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="TA_doc_num" type="xsd:string" nillable="true">
							<xsd:annotation>
								<xsd:documentation>Входящий номер документа у трансфер-агента	</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="TA_doc_date" type="date_or_datetime_t" nillable="true">
							<xsd:annotation>
								<xsd:documentation>Дата регистрации документа у трансфер-агента
										</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="doc_name" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Наименование документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="doc_edo" type="yes_no_t">
				<xsd:annotation>
					<xsd:documentation>Признак - документ поступил к регистратору в электронном виде</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="message_type" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Тип сообщения, которое было передано от трансфер-агента к регистратору, заполняется при doc_edo=yes</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="sender" type="doc_sender_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Отправитель документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="issuer" type="issuer_info_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Информация об эмитенте</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="doc_state" type="doc_processing_et">
				<xsd:annotation>
					<xsd:documentation>Состояние документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="process_date" type="date_or_datetime_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дата обработки документа / выполнения отказа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="outdoc_info" type="outdoc_info_t" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Данные исходящего документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="uid" type="uuid_t" use="optional"/>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="doc_link_t">
		<xsd:annotation>
			<xsd:documentation>Ссылка на документ (информация об инициирующем документе)</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="out_doc_num" type="xsd:string" nillable="true">
				<xsd:annotation>
					<xsd:documentation>Исходящий номер документа у отправителя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="out_doc_date" type="date_or_datetime_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дата создания документа у отправителя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="in_doc_num" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Входящий регистрационный номер</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="in_reg_date" type="date_or_datetime_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дата регистрации входящего документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="doc_link2_t">
		<xsd:annotation>
			<xsd:documentation>Ссылка на документ, который может быть еще не зарегистрирован другой стороной с указанием уникального идентификатора</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="out_doc_num" type="xsd:string" nillable="true">
				<xsd:annotation>
					<xsd:documentation>Исходящий номер документа у отправителя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="out_doc_date" type="date_or_datetime_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дата создания документа у отправителя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="in_doc_num" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Входящий регистрационный номер</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="in_reg_date" type="date_or_datetime_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дата регистрации входящего документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ta_doc_num" type="xsd:string" nillable="false" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Входящий номер документа у трансфер-агента</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ta_doc_date" type="date_or_datetime_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дата регистрации документа у трансфер-агента</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="uid" type="uuid_t" use="required">
			<xsd:annotation>
				<xsd:documentation>Уникальный идентификатор документа</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="doc_sender_t">
		<xsd:annotation>
			<xsd:documentation>Отправитель документа</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="sender_type" type="sender_type_et">
				<xsd:annotation>
					<xsd:documentation>Тип отправителя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="account_number" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Номер лицевого счета </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="account_type" type="account_type_et" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Вид счета</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="person_info" type="person_info_t">
				<xsd:annotation>
					<xsd:documentation>Информация об отправителе (наименование и данные документа)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="post_address" type="xsd:string" nillable="false" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Почтовый адрес отправителя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="person_sign" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Лицо, подписавшее сопроводительное письмо (для отправителей - юр.лиц)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="doc_type_t">
		<xsd:annotation>
			<xsd:documentation>Тип входящего документа</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="doc_type_code" type="doc_type_et">
				<xsd:annotation>
					<xsd:documentation>Код типа документа</xsd:documentation>
					<!-- ===================================================
Код вида обременения, должен принимать значения из фиксированного перечня. 
=================================================== -->
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="narrative" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Наименование документа</xsd:documentation>
					<!-- ===================================================
Заполняется обязательно, только в случае, если значением кода вида обременения указано OTHR
=================================================== -->
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="du_conditions_t">
		<xsd:annotation>
			<xsd:documentation>Условия доверительного управления</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="contract_num" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>номер договора ДУ</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="contract_date" type="xsd:date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>дата договора ДУ</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="right_du_nodispose" type="yes_no_t">
				<xsd:annotation>
					<xsd:documentation>ДУ не имеет права распоряжаться ценными бумагами</xsd:documentation>
					<!-- ===================================================
Полное официальное наименование  юридического лица - органа, осуществившего регистрацию
=================================================== -->
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="right_founder_voting" type="yes_no_t">
				<xsd:annotation>
					<xsd:documentation>право голоса принадлежит учредителю </xsd:documentation>
					<!-- ===================================================
Полное официальное наименование  юридического лица - органа, осуществившего регистрацию
=================================================== -->
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="right_founder_divid" type="yes_no_t">
				<xsd:annotation>
					<xsd:documentation>право получения дохода принадлежит учредителю </xsd:documentation>
					<!-- ===================================================
Полное официальное наименование  юридического лица - органа, осуществившего регистрацию
=================================================== -->
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="file_reference_t">
		<xsd:annotation>
			<xsd:documentation>Тип: ссылка на файл</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="type" type="file_type_et">
				<xsd:annotation>
					<xsd:documentation>Тип файла</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="description" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Описание файла</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="hash" type="xsd:base64Binary">
				<xsd:annotation>
					<xsd:documentation>Хэш файла</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="size" type="xsd:long">
				<xsd:annotation>
					<xsd:documentation>Размер файла (в байтах)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="foreign_doc_type_t">
		<xsd:annotation>
			<xsd:documentation>Тип документа, подтверждающего право пребывания в РФ</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="foreign_doc_type" type="foreign_document_type_et">
				<xsd:annotation>
					<xsd:documentation>тип документа, подтверждающего право пребывания в РФ</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="narrative" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>текстовое описание  типа документа</xsd:documentation>
					<!-- ===================================================
						Заполняется только в том случае, если код указан 91
						=================================================== -->
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="foreign_document_t">
		<xsd:annotation>
			<xsd:documentation>Данные миграционной карты или документа, подтверждающего право пребывания в РФ</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="doc_type" type="foreign_doc_type_t">
				<xsd:annotation>
					<xsd:documentation>тип документа, подтверждающего право пребывания в РФ</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="doc_series" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>серия документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="doc_num" type="xsd:string" nillable="true">
				<xsd:annotation>
					<xsd:documentation>номер документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="beg_date" type="xsd:date" nillable="true">
				<xsd:annotation>
					<xsd:documentation>дата начала срока пребывания в РФ</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="end_date" type="xsd:date" nillable="true">
				<xsd:annotation>
					<xsd:documentation>дата окончания срока пребывания в РФ</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="foreign_public_t">
		<xsd:annotation>
			<xsd:documentation>принадлежность к иностранному публичному должностному лицу</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="official" type="yes_no_t">
				<xsd:annotation>
					<xsd:documentation>признак, является ли лицо иностранным публичным должностным лицом</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="relative" type="yes_no_t">
				<xsd:annotation>
					<xsd:documentation>признак, является ли лицо супругом, близким родственником (родственником по прямой восходящей и нисходящей линии (родителем и ребенком, дедушкой, бабушкой и внуком), полнородным и неполнородным (имеющим общих отца или мать) братом и сестрой, усыновителем и усыновленным) иностранного публичного должностного лица</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="comment" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>комментарий</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="form_coownership_t">
		<xsd:annotation>
			<xsd:documentation>Анкета счёта общей собственности</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="owners_type" type="coowners_type_et">
				<xsd:annotation>
					<xsd:documentation>вид собственности</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="coowners_name">
				<xsd:annotation>
					<xsd:documentation>полное наименование счёта ОС</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="coowners_short_name">
				<xsd:annotation>
					<xsd:documentation>краткое наименование счёта ОС</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="coowners_post_address" type="address_post_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>почтовый адрес для направления корреспонденции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="form_individual_t">
		<xsd:annotation>
			<xsd:documentation>Анкета физического лица</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="individual_name" nillable="false">
				<xsd:annotation>
					<xsd:documentation>Фамилия, имя, отчество физического лица</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="individual_short_name" nillable="false" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Фамилия И.О. </xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="individual_document" type="individual_document_t">
				<xsd:annotation>
					<xsd:documentation>документ, удостоверяющий личность физического лица</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="birthtday" type="xsd:date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дата рождения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="place_of_birth" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>место рождения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="nationality" type="country_et" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>гражданство</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="foreign_document" type="foreign_document_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>данные миграционной карты или документа, подтверждающего право пребывания в РФ</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="individual_legal_address" type="address_legal_t" nillable="true">
				<xsd:annotation>
					<xsd:documentation>адрес постоянного места жительства/ адрес постоянной регистрации</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="individual_post_address" type="address_post_t" nillable="true">
				<xsd:annotation>
					<xsd:documentation>почтовый адрес</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="inn" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>код ИНН</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ssn" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>СНИЛС</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="additional_document" type="doc_indiv_business_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>информация о регистрации индивидуального предпринимателя </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="foreign_public" type="foreign_public_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>принадлежность к иностранному публичному должностному лицу</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="russian_public" type="russian_public_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>принадлежность к российскому публичному должностному лицу</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="owner_status" type="owner_status_et" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Статус ЗЛ </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="benefit" type="official_info_t" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Бенефициарные владельцы, выгодоприобретатели </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="form_juridical_t">
		<xsd:annotation>
			<xsd:documentation>Анкета юридического лица</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="juridical_name" nillable="false">
				<xsd:annotation>
					<xsd:documentation>полное наименование юридического лица</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="juridical_short_name" nillable="true">
				<xsd:annotation>
					<xsd:documentation>краткое наименование юридического лица</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="juridical_registration" type="juridical_registration_t">
				<xsd:annotation>
					<xsd:documentation>информация о регистрации юридического лица</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="juridical_legal_address" type="address_legal_t" nillable="true">
				<xsd:annotation>
					<xsd:documentation>адрес места нахождения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="juridical_post_address" type="address_post_t" nillable="true">
				<xsd:annotation>
					<xsd:documentation>почтовый адрес</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="registration_country" type="country_et" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>страна регистрации (инкорпорации) юридического лица</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="special" type="juridical_special_et" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>особый вид ЮЛ</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="inn" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>код ИНН (КИО)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="kpp" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>код КПП</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="okpo" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>код ОКПО</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:choice>
				<xsd:element name="okved" type="xsd:string" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>код ОКВЭД</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="okved2" type="xsd:string" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>код ОКВЭД2</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:choice>
			<xsd:element name="opf" type="opf_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Организационно-правовая форма</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="official" type="official_info_t" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Должностные лица, имеющие право действовать от имени ЮЛ без доверенности; бенефициары, выгодоприобретатели </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="licence" type="licence_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Данные  лицензии для счета НД, ДУ</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="benefit_noident" type="yes_no_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Идентификация бенефициарного владельца не требуется в силу закона</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="profit_noident" type="yes_no_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Идентификация выгодоприобретателя не требуется  в силу закона </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="format_report_t">
		<xsd:annotation>
			<xsd:documentation>Формат отчета</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="format" type="format_report_et">
				<xsd:annotation>
					<xsd:documentation>Формат отчета (значение из фиксированного справочника)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="mime_type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>MIME тип. Кодируется по стандарту RFC 2046 и поддерживается IANA</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="id_t">
		<xsd:annotation>
			<xsd:documentation>Идентификатор объекта</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="id" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Идентификатор</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="organization" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Наименование организации, присвоившей идентификатор</xsd:documentation>
					<!-- ===================================================
Должен быть указан один из элементов (issuer или scheme_name)
Могут указываться коды, организаций, включенных в справочник Data Source Scheme, используемый SWIFT
=================================================== -->
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="id_name_t">
		<xsd:annotation>
			<xsd:documentation>Идентификатор объекта и его наименование</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="id" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Идентификатор</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="name" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Наименование</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="organization" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Наименование организации, присвоившей идентификатор</xsd:documentation>
					<!-- ===================================================
Должен быть указан один из элементов (issuer или scheme_name)
Могут указываться коды, организаций, включенных в справочник Data Source Scheme, используемый SWIFT
=================================================== -->
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="individual_doc_type_t">
		<xsd:annotation>
			<xsd:documentation>Тип документа, удостоверяющего личность физического лица</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="individual_doc_type" type="individual_document_type_et">
				<xsd:annotation>
					<xsd:documentation>тип документа, удостоверяющего личность </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="narrative" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>текстовое описание  типа документа</xsd:documentation>
					<!-- ===================================================
						Заполняется только в том случае, если код указан 91
						=================================================== -->
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="individual_document_t">
		<xsd:annotation>
			<xsd:documentation>Документ, удостоверяющий личность физического лица</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="doc_type" type="individual_doc_type_t">
				<xsd:annotation>
					<xsd:documentation>тип документа, удостоверяющего личность </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="doc_series" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>серия документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="doc_num" type="xsd:string" nillable="true">
				<xsd:annotation>
					<xsd:documentation>номер документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="doc_date" type="xsd:date" nillable="true">
				<xsd:annotation>
					<xsd:documentation>дата выдачи документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="doc_place" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>место выдачи документа</xsd:documentation>
					<!-- ===================================================
Полное официальное наименование  юридического лица - органа, осуществившего регистрацию
=================================================== -->
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="doc_subcode" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>код подразделения выдачи паспорта</xsd:documentation>
					<!-- ===================================================
Полное официальное наименование  юридического лица - органа, осуществившего регистрацию
=================================================== -->
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="individual_info_t">
		<xsd:annotation>
			<xsd:documentation>Характеристика физического лица</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="individual_name" nillable="false">
				<xsd:annotation>
					<xsd:documentation>фамилия, имя, отчество </xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="individual_document" type="individual_document_t">
				<xsd:annotation>
					<xsd:documentation>документ, удостоверяющий личность</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="info_analytics_t">
		<xsd:annotation>
			<xsd:documentation>Доп.информация анкеты - аналитика эмитента (вложенный блок в add_info)</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="issuer_subdiv" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Подразделение эмитента</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="id" type="id_t">
							<xsd:annotation>
								<xsd:documentation>Код подразделения эмитента</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="narrative" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Наименование подразделения эмитента</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="issuer_categ" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Категория акционера в классификации эмитента</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="id" type="id_t">
							<xsd:annotation>
								<xsd:documentation>Код категории акционера</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="narrative" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Наименование категории акционера</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="fl_worker" type="yes_no_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Признак работника АО (эмитента)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="tabel" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Табельный номер</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="issuer_info_t">
		<xsd:annotation>
			<xsd:documentation>Информация об эмитенте</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="issuer_id" type="id_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Идентификатор эмитента</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="issuer_info" type="juridical_info_t">
				<xsd:annotation>
					<xsd:documentation>Информация об эмитенте</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="issuer_legal_address" type="address_legal_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Адрес места нахождения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="juridical_info_t">
		<xsd:annotation>
			<xsd:documentation>Характеристика юридического лица</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="juridical_name" nillable="false">
				<xsd:annotation>
					<xsd:documentation>полное наименование юридического лица</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="juridical_registration" type="juridical_registration_t">
				<xsd:annotation>
					<xsd:documentation>информация о регистрации юридического лица</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="juridical_ogrn_t">
		<xsd:annotation>
			<xsd:documentation>ОГРН юридического лица</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="ogrn" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>ОГРН</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ogrn_date" type="xsd:date" nillable="true">
				<xsd:annotation>
					<xsd:documentation>дата выдачи ОГРН</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ogrn_place" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>орган, осуществивший регистрацию</xsd:documentation>
					<!-- ===================================================
Полное официальное наименование  юридического лица - органа, осуществившего регистрацию
=================================================== -->
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="juridical_doc_type_t">
		<xsd:annotation>
			<xsd:documentation>Тип документа регистрации юридического лица</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="juridical_doc_type" type="juridical_reg_doc_type_et">
				<xsd:annotation>
					<xsd:documentation>тип документа о регистрации юридического лица</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="narrative" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>текстовое описание типа документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="juridical_reg_doc_t">
		<xsd:annotation>
			<xsd:documentation>Документ регистрации юридического лица</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="reg_doc_type" type="juridical_doc_type_t">
				<xsd:annotation>
					<xsd:documentation>тип документа о регистрации юридического лица</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="reg_num" type="xsd:string" nillable="true">
				<xsd:annotation>
					<xsd:documentation>номер регистрации</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="reg_series" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>серия документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="reg_date" type="xsd:date" nillable="true">
				<xsd:annotation>
					<xsd:documentation>дата регистрации</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="reg_place" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>орган, осуществивший регистрацию</xsd:documentation>
					<!-- ===================================================
Полное официальное наименование  юридического лица - органа, осуществившего регистрацию
=================================================== -->
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="juridical_registration_t">
		<xsd:annotation>
			<xsd:documentation>Регистрация юридического лица</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="ogrn" type="juridical_ogrn_t" nillable="false" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>ОГРН </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="reg_doc" type="juridical_reg_doc_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>документ гос.регистрации (для юридических лиц, зарегистрированных до 01.07.2002 и нерезидентов)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="licence_t">
		<xsd:annotation>
			<xsd:documentation>Данные лицензии зарегистрированного лица</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="lic_name" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>наименование лицензии</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="lic_num" type="xsd:string" nillable="true">
				<xsd:annotation>
					<xsd:documentation>номер лицензии</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="lic_date" type="xsd:date" nillable="true">
				<xsd:annotation>
					<xsd:documentation>дата выдачи лицензии</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="lic_place" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>орган, выдавший лицензию</xsd:documentation>
					<!-- ===================================================
Полное официальное наименование  юридического лица - органа, осуществившего регистрацию
=================================================== -->
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="lic_end" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Окончание действия лицензии</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="unlimited" type="yes_no_t">
							<xsd:annotation>
								<xsd:documentation>Признак бессрочной лицензии</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="end_date" type="xsd:date" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Дата окончания действия</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="msg_t" abstract="true">
		<xsd:annotation>
			<xsd:documentation>Тип: сообщение</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:annotation>
				<xsd:documentation>Стороны электронного документооборота (регистратор и абонент)</xsd:documentation>
			</xsd:annotation>
			<xsd:element name="msg_sender" type="party_exchange_info_t">
				<xsd:annotation>
					<xsd:documentation>Отправитель сообщения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="msg_recipient" type="party_exchange_info_t">
				<xsd:annotation>
					<xsd:documentation>Получатель сообщения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="version" type="xsd:string" use="required" fixed="fes-1.0" form="unqualified">
			<xsd:annotation>
				<xsd:documentation>Версия формата сообщений</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="uid" type="uuid_t" use="required">
			<xsd:annotation>
				<xsd:documentation>Идентификатор сообщения</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="official_form_individual_t">
		<xsd:annotation>
			<xsd:documentation>Данные физического лица - должностного лица, бенефициара, выгодоприобретателя</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="birthtday" type="xsd:date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дата рождения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="place_of_birth" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>место рождения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="nationality" type="country_et" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>гражданство</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="foreign_document" type="foreign_document_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>данные миграционной карты или документа, подтверждающего право пребывания в РФ</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="address" type="address_nostruct_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Адрес</xsd:documentation>
					<!-- ===================================================
По согласованию сторон используется один из вариантов
=================================================== -->
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="inn" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Код ИНН</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="foreign_public" type="foreign_public_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>принадлежность к иностранному публичному должностному лицу</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="russian_public" type="russian_public_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>принадлежность к российскому публичному должностному лицу</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="official_form_juridical_t">
		<xsd:annotation>
			<xsd:documentation>Данные юридического лица - должностного лица, бенефициара, выгодоприобретателя</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="nationality" type="country_et" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>страна регистрации</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="address" type="address_nostruct_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Адрес</xsd:documentation>
					<!-- ===================================================
По согласованию сторон используется один из вариантов
=================================================== -->
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="inn" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Код ИНН</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="official" type="official_info_t" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Должностные лица, имеющие право действовать от имени ЮЛ без доверенности; бенефициары, выгодоприобретатели </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="official_form_t" block="extension" final="extension">
		<xsd:annotation>
			<xsd:documentation>Данные должностного лица, бенефициара, выгодоприобретателя</xsd:documentation>
		</xsd:annotation>
		<xsd:choice>
			<xsd:element name="juridical" type="official_form_juridical_t">
				<xsd:annotation>
					<xsd:documentation>Дополнительные данные юридического лица</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="individual" type="official_form_individual_t">
				<xsd:annotation>
					<xsd:documentation>Дополнительные данные физического лица</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:choice>
	</xsd:complexType>
	<xsd:complexType name="official_info_t">
		<xsd:annotation>
			<xsd:documentation>Должностное лицо, действующее от имени ЮЛ без доверенности; бенефициар, выгодоприобретатель </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="official_info" type="authorised_person_info_t">
				<xsd:annotation>
					<xsd:documentation>Основные данные  (наименование и документ) </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="official_form" type="official_form_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дополнительные данные </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="contacts" type="contacts_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Контактная информация</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="comment" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Доп.информация</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="premier" type="yes_no_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Признак единоличного исполнительного органа (первого лица)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="premier_benefit" type="yes_no_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Признак: ЕИО является бенефициаром</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="operation_t">
		<xsd:annotation>
			<xsd:documentation>Описание операции в реестре (полное)</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="base_doc" type="base_doc_t">
				<xsd:annotation>
					<xsd:documentation>Документ-основание </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="processing_info">
				<xsd:annotation>
					<xsd:documentation>Данные об обработке операции</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="id_operation" type="xsd:long" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Уникальный идентификатор операции</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="operation_name" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Наименование операции</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="operation_numb" type="xsd:nonNegativeInteger">
							<xsd:annotation>
								<xsd:documentation>Номер операции в регистрационном журнале</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="operation_date" type="xsd:date">
							<xsd:annotation>
								<xsd:documentation>Дата проведения операции </xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:choice minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Описание операции</xsd:documentation>
				</xsd:annotation>
				<xsd:element name="account_open" type="operation_form_t">
					<xsd:annotation>
						<xsd:documentation>Открытие счета</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="account_modify" type="operation_form_t">
					<xsd:annotation>
						<xsd:documentation>Изменение анкеты ЗЛ</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="transaction" type="operation_transaction_t">
					<xsd:annotation>
						<xsd:documentation>Переход прав собственности (передача ЦБ)</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="block" type="operation_block_t">
					<xsd:annotation>
						<xsd:documentation>Блокирование ценных бумаг</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="unblock" type="operation_unblock_t">
					<xsd:annotation>
						<xsd:documentation>Снятие блокирования ЦБ</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="pledge" type="operation_pledge_t">
					<xsd:annotation>
						<xsd:documentation>Установка залога</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="unpledge" type="operation_unpledge_t">
					<xsd:annotation>
						<xsd:documentation>Снятие залога</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="pledge_modify" type="operation_pledge_modify_t">
					<xsd:annotation>
						<xsd:documentation>Изменение условий залога</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="pledge_deliver" type="operation_pledge_deliver_t">
					<xsd:annotation>
						<xsd:documentation>Передача права залога</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="unpaid" type="operation_unpaid_t">
					<xsd:annotation>
						<xsd:documentation>Установка обременения обязательством оплаты</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="unpaid_cancel" type="operation_unpaid_cancel_t">
					<xsd:annotation>
						<xsd:documentation>Снятие обременения обязательством оплаты</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="blockoper" type="operation_blockoper_t">
					<xsd:annotation>
						<xsd:documentation>Установка блокирования операций</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="unblockoper" type="operation_blockoper_t">
					<xsd:annotation>
						<xsd:documentation>Снятие блокирования операций</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="convert" type="operation_convert_t">
					<xsd:annotation>
						<xsd:documentation>Конвертация ЦБ по одному счету</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="coowner_open" type="operation_coowner_t">
					<xsd:annotation>
						<xsd:documentation>Ввод совладельца</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="coowner_modify" type="operation_coowner_t">
					<xsd:annotation>
						<xsd:documentation>Изменение анкеты совладельца</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="coowner_close" type="operation_coowner_close_t">
					<xsd:annotation>
						<xsd:documentation>Удаление совладельца из счета ОС</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="coowner_quota" type="operation_coowners_quota_t">
					<xsd:annotation>
						<xsd:documentation>Изменение доли совладельца</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="proxy_open" type="operation_proxy_t">
					<xsd:annotation>
						<xsd:documentation>Ввод представителя</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="proxy_modify" type="operation_proxy_t">
					<xsd:annotation>
						<xsd:documentation>Изменение анкеты или полномочий представителя</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="proxy_cancel" type="operation_proxy_cancel_t">
					<xsd:annotation>
						<xsd:documentation>Отмена полномочий представителя</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="account_close" type="operation_account_close_t">
					<xsd:annotation>
						<xsd:documentation>Закрытие счета</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="founder_open" type="operation_founder_t">
					<xsd:annotation>
						<xsd:documentation>Ввод учредителя ДУ</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="founder_modify" type="operation_founder_t">
					<xsd:annotation>
						<xsd:documentation>Изменение анкеты учредителя ДУ</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="founder_close" type="operation_founder_close_t">
					<xsd:annotation>
						<xsd:documentation>Удаление учредителя ДУ</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:choice>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="operation_brief_t">
		<xsd:annotation>
			<xsd:documentation>Описание операции краткое </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="base_doc" type="base_doc_t">
				<xsd:annotation>
					<xsd:documentation>Документ-основание </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="processing_info" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Данные об обработке операции</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="id_operation" type="xsd:long" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Уникальный идентификатор операции</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="operation_name" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Наименование операции</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="operation_numb" type="xsd:positiveInteger">
							<xsd:annotation>
								<xsd:documentation>Номер операции в регистрационном журнале</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="operation_date" type="xsd:date">
							<xsd:annotation>
								<xsd:documentation>Дата проведения операции </xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="security" type="security_info_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Идентификатор ЦБ</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="quantity" type="units_or_fraction_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Количество ЦБ</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="operation_account_close_t">
		<xsd:annotation>
			<xsd:documentation>Операция закрытия счета</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="account_ident" type="account_ident_t">
				<xsd:annotation>
					<xsd:documentation>Зарегистрированное лицо, счет которого закрывается</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="authorised_person" type="authorised_person_info_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Уполномоченный представитель ЗЛ (обязателен для юридических лиц)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="operation_coowner_t">
		<xsd:annotation>
			<xsd:documentation>Операция ввода совладельца / изменения анкеты совладельца / изменения доли совладельца</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="account_ident" type="account_ident_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Идентификатор счета ОС, которому принадлежит совладелец (для добавления совладельца и изменения его анкеты) </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="coowner_ident" type="coowner_info_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Идентификатор совладельца, анкета которого изменяется (только для изменения анкеты совладельца)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="coowner_form" type="content_form_of_coowner_t">
				<xsd:annotation>
					<xsd:documentation>Анкета совладельца</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="quota" type="coowner_quota_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Доля совладельца (только для счетов ОС с долевой собственностью) </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="operation_coowner_close_t">
		<xsd:annotation>
			<xsd:documentation>Операция удаления совладельца</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="account_ident" type="account_ident_t">
				<xsd:annotation>
					<xsd:documentation>Идентификатор счета ОС, которому принадлежит совладелец </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="coowner_ident" type="coowner_info_t">
				<xsd:annotation>
					<xsd:documentation>Идентификатор совладельца, который удаляется из счета ОС</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="operation_coowners_quota_t">
		<xsd:annotation>
			<xsd:documentation>Операция установки/изменения долей совладельцев</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="account_ident" type="account_ident_t">
				<xsd:annotation>
					<xsd:documentation>Идентификатор счета ОС, которому принадлежит совладелец </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="coowner_quota" type="coowner_info_quota_t" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Совладелец и его доля</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="operation_form_t">
		<xsd:annotation>
			<xsd:documentation>Операция открытия счета / изменения анкеты ЗЛ</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="account_ident" type="account_ident_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Идентификатор счета ЗЛ, по которому выполняется изменение анкетных данных (только для изменения анкетных данных)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="shareholder_form" type="content_form_of_shareholders_t">
				<xsd:annotation>
					<xsd:documentation>Анкета ЗЛ</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="add_trustee" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дополнительные данные для счета ДУ</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="profi" type="yes_no_t">
							<xsd:annotation>
								<xsd:documentation>ДУ - профучастник рынка ЦБ</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="founder_form" type="content_form_of_shareholders_t" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Анкета учредителя </xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="du_conditions" type="du_conditions_t" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Договор и условия доверительногго управления</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="operation_founder_t">
		<xsd:annotation>
			<xsd:documentation>Операция ввода учредителя ДУ / изменения анкеты учредителя</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="account_ident" type="account_ident_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Идентификатор счета ЗЛ, по которому выполняется изменение анкетных данных (только для изменения анкетных данных)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="founder_form" type="content_form_of_shareholders_t">
				<xsd:annotation>
					<xsd:documentation>Анкета учредителя </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="du_conditions" type="du_conditions_t">
				<xsd:annotation>
					<xsd:documentation>Договор и условия доверительногго управления</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="operation_founder_close_t">
		<xsd:annotation>
			<xsd:documentation>Операция удаления учредителя ДУ</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="account_ident" type="account_ident_t">
				<xsd:annotation>
					<xsd:documentation>Идентификатор счета ДУ, у которого удаляется учредитель</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="operation_proxy_t">
		<xsd:annotation>
			<xsd:documentation>Операция ввода представителя </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="account_ident" type="account_ident_t">
				<xsd:annotation>
					<xsd:documentation>Идентификатор лицевого счета ЗЛ, которому принадлежит представитель</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ident_authorised_person" type="authorised_person_ident_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Идентификатор уполномоченного представителя, по которому выполняется изменение данных (только для изменения анкетных данных и полномочий)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="authorised_person_type" type="authorised_person_type_et">
				<xsd:annotation>
					<xsd:documentation>тип уполномоченного представителя (кроме кодов 001,004 - это официальные лица юр.лица, они указываются в анкете ЗЛ)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="authorised_person_form" type="content_form_of_authorised_t">
				<xsd:annotation>
					<xsd:documentation>Анкета представителя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="powers">
				<xsd:annotation>
					<xsd:documentation>Полномочия представителя</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="bases_of_powers" type="bases_of_powers_t">
							<xsd:annotation>
								<xsd:documentation>Основание для полномочий (например, доверенность)</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:sequence>
							<xsd:annotation>
								<xsd:documentation>Права представителя</xsd:documentation>
							</xsd:annotation>
							<xsd:element name="rights_signature" type="yes_no_t">
								<xsd:annotation>
									<xsd:documentation>Право на подписание документов на проведение операций в реестре</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="rights_voting" type="yes_no_t">
								<xsd:annotation>
									<xsd:documentation>Право голосования</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="rights_dividend" type="yes_no_t">
								<xsd:annotation>
									<xsd:documentation>Право на получение дивидендов</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="rights_docum" type="yes_no_t">
								<xsd:annotation>
									<xsd:documentation>Право на получение информации из реестра</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="rights_courier" type="yes_no_t">
								<xsd:annotation>
									<xsd:documentation>Право на подачу документов</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
						<xsd:element name="comment" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Комментарий к правам</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="operation_proxy_cancel_t">
		<xsd:annotation>
			<xsd:documentation>Операция отмены полномочий представителя </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="account_ident" type="account_ident_t">
				<xsd:annotation>
					<xsd:documentation>Идентификатор лицевого счета ЗЛ, которому принадлежит представитель</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ident_authorised_person" type="authorised_person_ident_t">
				<xsd:annotation>
					<xsd:documentation>Идентификатор уполномоченного представителя, по которому выполняется отмена полномочий</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="operation_block_t">
		<xsd:annotation>
			<xsd:documentation>Операция установки блокирования ЦБ</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="account_owner" type="account_ident_t">
				<xsd:annotation>
					<xsd:documentation>Зарегистрированное лицо, на счете которого блокируются ЦБ</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="authorised_person_owner" type="authorised_person_info_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Уполномоченный представитель ЗЛ (обязателен для юридических лиц)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="block_info" type="block_info_t">
				<xsd:annotation>
					<xsd:documentation>Описание блокирования ЦБ</xsd:documentation>
					<!-- ===================================================
 Не подлежит заполнению, т.к. признак наличия обременения может быть No
=================================================== -->
				</xsd:annotation>
			</xsd:element>
			<xsd:choice minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Для блокирования  обремененных ЦБ</xsd:documentation>
				</xsd:annotation>
				<xsd:element name="block_prev" type="block_info_t">
					<xsd:annotation>
						<xsd:documentation>Сведения о предыдущем блокировании (для установки блокирования блокированных ЦБ)</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="pledge_prev" type="pledge_info_t">
					<xsd:annotation>
						<xsd:documentation>Сведения о предыдущем залоге (для установки блокирования заложенных ЦБ)</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="unpaid_prev" type="unpaid_info_t">
					<xsd:annotation>
						<xsd:documentation>Сведения о предыдущем обременении обязательством оплаты (для блокирования неоплаченных ЦБ) </xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:choice>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="operation_blockoper_t">
		<xsd:annotation>
			<xsd:documentation>Операция установки блокирования операций по счету </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="account_owner" type="account_ident_t">
				<xsd:annotation>
					<xsd:documentation>Зарегистрированное лицо, операции которого блокируются</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="authorised_person_owner" type="authorised_person_info_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Уполномоченный представитель ЗЛ (обязателен для юридических лиц)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="blockoper_info" type="blockoper_info_t">
				<xsd:annotation>
					<xsd:documentation>Описание блокирования операций</xsd:documentation>
					<!-- ===================================================
 Не подлежит заполнению, т.к. признак наличия обременения может быть No
=================================================== -->
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="operation_convert_t">
		<xsd:annotation>
			<xsd:documentation>Операция конвертации по одному счету</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="account_owner" type="account_ident_t">
				<xsd:annotation>
					<xsd:documentation>Зарегистрированное лицо, на счете которого конвертируются ЦБ</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="authorised_person_owner" type="authorised_person_ident_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Уполномоченный представитель лица, передающего ЦБ (обязателен для юридических лиц)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="security_deliv" type="security_info_t">
				<xsd:annotation>
					<xsd:documentation>Идентификатор ЦБ списания</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="security_receip" type="security_info_t">
				<xsd:annotation>
					<xsd:documentation>Идентификатор ЦБ зачисления</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="quantity" type="units_or_fraction_t">
				<xsd:annotation>
					<xsd:documentation>Количество конвертируемых ЦБ</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:sequence>
				<xsd:annotation>
					<xsd:documentation>Коэффициент конвертации</xsd:documentation>
				</xsd:annotation>
				<xsd:element name="numerator" type="xsd:positiveInteger">
					<xsd:annotation>
						<xsd:documentation>Числитель</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="denominator" type="xsd:positiveInteger">
					<xsd:annotation>
						<xsd:documentation>Знаменатель</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="operation_unblock_t">
		<xsd:annotation>
			<xsd:documentation>Операция снятия блокирования ЦБ</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="account_owner" type="account_ident_t">
				<xsd:annotation>
					<xsd:documentation>Зарегистрированное лицо, на счете которого блокированы ЦБ</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="authorised_person_owner" type="authorised_person_info_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Уполномоченный представитель ЗЛ (обязателен для юридических лиц)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="block_info" type="block_info_t">
				<xsd:annotation>
					<xsd:documentation>Описание блокирования ЦБ</xsd:documentation>
					<!-- ===================================================
 Не подлежит заполнению, т.к. признак наличия обременения может быть No
=================================================== -->
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="quantity_unblock" type="units_or_fraction_t">
				<xsd:annotation>
					<xsd:documentation>Количество ЦБ, на которое снимается блокирование (меньше или равно  количеству блокированных ЦБ)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="operation_pledge_t">
		<xsd:annotation>
			<xsd:documentation>Операция установки залога ЦБ</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="account_owner" type="account_ident_t">
				<xsd:annotation>
					<xsd:documentation>Залогодатель</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="authorised_person_owner" type="authorised_person_info_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Уполномоченный представитель залогодателя (обязателен для юридических лиц)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="pledge_info" type="pledge_info_t">
				<xsd:annotation>
					<xsd:documentation>Описание залога</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:choice minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Для залога обремененных ЦБ</xsd:documentation>
				</xsd:annotation>
				<xsd:element name="pledge_prev" type="pledge_info_t">
					<xsd:annotation>
						<xsd:documentation>Сведения о предыдущем залоге (для установки повторного залога)</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="unpaid_prev" type="unpaid_info_t">
					<xsd:annotation>
						<xsd:documentation>Сведения о предыдущем обременении обязательством оплаты (для залога неоплаченных ЦБ) </xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:choice>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="operation_pledge_modify_t">
		<xsd:annotation>
			<xsd:documentation>Операция изменения условий залога ЦБ</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="account_owner" type="account_ident_t">
				<xsd:annotation>
					<xsd:documentation>Залогодатель</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="authorised_person_owner" type="authorised_person_info_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Уполномоченный представитель залогодателя (обязателен для юридических лиц)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="pledge_info" type="pledge_info_t">
				<xsd:annotation>
					<xsd:documentation>Описание залога</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:sequence>
				<xsd:annotation>
					<xsd:documentation>Измененные данные залога</xsd:documentation>
				</xsd:annotation>
				<xsd:element name="condition_pledge_new" type="condition_pledge_t">
					<xsd:annotation>
						<xsd:documentation>Измененнные условия залога</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="comment" type="xsd:string" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>Комментарий к описанию залога</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="operation_pledge_deliver_t">
		<xsd:annotation>
			<xsd:documentation>Операция передачи права залога ЦБ</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="account_owner" type="account_ident_t">
				<xsd:annotation>
					<xsd:documentation>Залогодатель</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="authorised_person_owner" type="authorised_person_info_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Уполномоченный представитель залогодателя (обязателен для юридических лиц)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="pledge_info" type="pledge_info_t">
				<xsd:annotation>
					<xsd:documentation>Описание залога</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="account_pledgee_new" type="account_ident_t">
				<xsd:annotation>
					<xsd:documentation>Залогодержатель, принимающий права залога </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="quantity_deliver" type="units_or_fraction_t">
				<xsd:annotation>
					<xsd:documentation>Количество заложенных ЦБ, права на которые передаются (меньше или равно количеству в залоге)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="based_info" type="based_info_t" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Основание для проведения операции (договор об уступке прав)</xsd:documentation>
					<!-- ===================================================
(повторяющийся элемент)
=================================================== -->
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="operation_transaction_t">
		<xsd:annotation>
			<xsd:documentation>Операция передачи прав собственности</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="account_deliv" type="account_ident_t">
				<xsd:annotation>
					<xsd:documentation>Зарегистрированное лицо, передающее ценные бумаги (счет списания)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="authorised_person_deliv" type="authorised_person_ident_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Уполномоченный представитель лица, передающего ЦБ (обязателен для юридических лиц)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="account_receiv" type="account_ident_t">
				<xsd:annotation>
					<xsd:documentation>Лицо, на счет которого должны быть зачислены ценные бумаги (счет зачисления)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="transaction" type="transaction_t">
				<xsd:annotation>
					<xsd:documentation>Описание сделки/ операции с ценными бумагами</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="operation_unpledge_t">
		<xsd:annotation>
			<xsd:documentation>Операция снятия залога ЦБ</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="account_owner" type="account_ident_t">
				<xsd:annotation>
					<xsd:documentation>Залогодатель</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="authorised_person_owner" type="authorised_person_info_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Уполномоченный представитель залогодателя (обязателен для юридических лиц)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="pledge_info" type="pledge_info_t">
				<xsd:annotation>
					<xsd:documentation>Описание залога</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="quantity_unpledge" type="units_or_fraction_t">
				<xsd:annotation>
					<xsd:documentation>Количество ЦБ, на которое снимается залог (меньше или равно количеству ЦБ в залоге)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:sequence minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Количественные показатели из условий залога, на которые уменьшаются указанные в условиях количества при частичном прекращении залога</xsd:documentation>
				</xsd:annotation>
				<xsd:element name="quantity_convert" type="units_or_fraction_t" nillable="false" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>Количество, на которое нужно уменьшить указанное в условиях количество ЦБ после конвертации (при convert=part)</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="quantity_divid" type="units_or_fraction_t" nillable="false" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>Количество, на которое нужно уменьшить указанное в условиях количество ЦБ, получателем дохода на которое является залогодержатель(при deleg_divid=part)</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="quantity_vote" type="units_or_fraction_t" nillable="false" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>Количество, на которое нужно уменьшить указанное в условиях количество ЦБ, правом голоса на которое обладает залогодержатель (при deleg_vote=part)</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="operation_unpaid_t">
		<xsd:annotation>
			<xsd:documentation>Операция установки обременения обязательства оплаты ЦБ</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="account_owner" type="account_ident_t">
				<xsd:annotation>
					<xsd:documentation>Зарегистрированное лицо, на счете которого устанавливается обременение</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="authorised_person_owner" type="authorised_person_info_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Уполномоченный представитель ЗЛ (обязателен для юридических лиц)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="unpaid_info" type="unpaid_info_t">
				<xsd:annotation>
					<xsd:documentation>Описание блокирования ЦБ</xsd:documentation>
					<!-- ===================================================
 Не подлежит заполнению, т.к. признак наличия обременения может быть No
=================================================== -->
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="operation_unpaid_cancel_t">
		<xsd:annotation>
			<xsd:documentation>Операция снятия обременения обязательством оплаты</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="account_owner" type="account_ident_t">
				<xsd:annotation>
					<xsd:documentation>Зарегистрированное лицо, на счете которого установлено обременение</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="authorised_person_owner" type="authorised_person_info_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Уполномоченный представитель ЗЛ (обязателен для юридических лиц)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="unpaid_info" type="unpaid_info_t">
				<xsd:annotation>
					<xsd:documentation>Описание обременения ЦБ обязательством оплаты</xsd:documentation>
					<!-- ===================================================
 Не подлежит заполнению, т.к. признак наличия обременения может быть No
=================================================== -->
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="quantity_cancel" type="units_or_fraction_t">
				<xsd:annotation>
					<xsd:documentation>Количество ЦБ, на которое снимается обременение (меньше или равно  количеству ЦБ в обременении)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="opf_t">
		<xsd:annotation>
			<xsd:documentation>Организационно-правовая форма ЮЛ</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="opf_type" type="opf_et">
				<xsd:annotation>
					<xsd:documentation>код ОПФ</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="narrative" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>текстовое описание  ОПФ</xsd:documentation>
					<!-- ===================================================
						Заполняется только в том случае, если код указан 00
						=================================================== -->
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="out_doc_t">
		<xsd:annotation>
			<xsd:documentation>Документ, который является ответом на запрос (ответный документ)</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="msg_t">
				<xsd:sequence>
					<xsd:element name="doc_num" type="xsd:string">
						<xsd:annotation>
							<xsd:documentation>Исходящий номер  документа										</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="doc_date" type="date_or_datetime_t">
						<xsd:annotation>
							<xsd:documentation>Дата документа
										</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="doc_link" type="doc_link_t" nillable="true">
						<xsd:annotation>
							<xsd:documentation>Ссылка на документ-основание</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="doc_delivery" type="delivery_doc_t" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Данные о выдаче исходящего документа </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="outdoc_info_t">
		<xsd:annotation>
			<xsd:documentation>Описание исходящего документа регистратора</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="outdoc_num" type="xsd:string" nillable="true">
				<xsd:annotation>
					<xsd:documentation>Исходящий номер  документа у регистратора										</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="outdoc_date" type="date_or_datetime_t">
				<xsd:annotation>
					<xsd:documentation>Дата создания исходящего документа					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="outdoc_name" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Наименование документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="report_mode" type="report_mode_et">
				<xsd:annotation>
					<xsd:documentation>Форма предоставления отчета (бумажный оригинал, электронный документ)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="message_type" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Тип сообщения, которое было передано от регистратора к трансфер-агенту</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="receipt_date" type="date_or_datetime_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дата получения исходящего документа трансфер-агентом  </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="delivery_state" type="yes_no_t">
				<xsd:annotation>
					<xsd:documentation>Отметка о выдаче документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="delivery_date" type="date_or_datetime_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дата выдачи</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="delivery" type="delivery_doc_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Данные о выдаче исходящего документа </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="party_exchange_info_t">
		<xsd:annotation>
			<xsd:documentation>Информация об участнике обмена</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="type_party" type="party_exchange_type_et">
				<xsd:annotation>
					<xsd:documentation>Тип участника обмена</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="name" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Наименование участника обмена
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="person_info_t" block="extension" final="extension">
		<xsd:annotation>
			<xsd:documentation>Идентификационные данные лица  (для ФЛ, ЮЛ, ОС)</xsd:documentation>
		</xsd:annotation>
		<xsd:choice>
			<xsd:element name="juridical" type="juridical_info_t">
				<xsd:annotation>
					<xsd:documentation>Характеристика юридического лица</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="individual" type="individual_info_t">
				<xsd:annotation>
					<xsd:documentation>Характеристика  физического лица</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="coowners" type="coownership_info_t">
				<xsd:annotation>
					<xsd:documentation>Характеристика   счета общей собственности</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:choice>
	</xsd:complexType>
	<xsd:complexType name="person_info2_t" block="extension" final="extension">
		<xsd:annotation>
			<xsd:documentation>Идентификационные данные лица  (для ФЛ, ЮЛ)</xsd:documentation>
		</xsd:annotation>
		<xsd:choice>
			<xsd:element name="juridical" type="juridical_info_t">
				<xsd:annotation>
					<xsd:documentation>Характеристика юридического лица</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="individual" type="individual_info_t">
				<xsd:annotation>
					<xsd:documentation>Характеристика  физического лица</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:choice>
	</xsd:complexType>
	<xsd:complexType name="phone_t">
		<xsd:annotation>
			<xsd:documentation>Номер телефона</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="phone_num" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>не структурированный номер телефона</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="phone_type" type="phone_type_et" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>код типа номера телефона</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<!-- ============================================================================================= -->
	</xsd:complexType>
	<xsd:complexType name="pledge_info_t">
		<xsd:annotation>
			<xsd:documentation>Описание залога ЦБ</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="id_pledge" type="xsd:nonNegativeInteger" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Уникальный номер обременения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="pledge_date" type="xsd:date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дата установки залога</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="pledge_type" type="block_type_t">
				<xsd:annotation>
					<xsd:documentation>Вид обременения (PLDG - залог ЦБ, PLDN - залог неоплаченных ЦБ)</xsd:documentation>
					<!-- ===================================================
 Не подлежит заполнению, т.к. признак наличия обременения может быть No
=================================================== -->
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="repeat_pledge" type="yes_no_t">
				<xsd:annotation>
					<xsd:documentation>Признак повторного залога</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="account_pledgee" type="account_ident_t">
				<xsd:annotation>
					<xsd:documentation>Залогодержатель </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="security" type="security_info_t">
				<xsd:annotation>
					<xsd:documentation>Ценная бумага</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="quantity" type="units_or_fraction_t">
				<xsd:annotation>
					<xsd:documentation>Количество ЦБ, переданных в залог</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="condition_pledge" type="condition_pledge_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Условия залога</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="comment" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Комментарий к описанию залога</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="based_info" type="based_info_t" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Основание для проведения операции (договор залога)</xsd:documentation>
					<!-- ===================================================
(повторяющийся элемент)
=================================================== -->
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="registrar_info_t">
		<xsd:annotation>
			<xsd:documentation>Информация о регистраторе</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="registrar_info" type="juridical_info_t">
				<xsd:annotation>
					<xsd:documentation>Информация о регистраторе (наименование и госрегистрация)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="registrar_legal_address" type="address_legal_t" nillable="true">
				<xsd:annotation>
					<xsd:documentation>Адрес места нахождения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="contacts" type="contacts_t">
				<xsd:annotation>
					<xsd:documentation>Контактная информация</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="reconciliation_info_t">
		<xsd:annotation>
			<xsd:documentation>Результат сверки </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="result_reconciliation" type="result_reconciliation_et">
				<xsd:annotation>
					<xsd:documentation>Результат сверки</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="narrative" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Описание причины расхождения  сверки, заполняется для причины DIVER </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="russian_public_t">
		<xsd:annotation>
			<xsd:documentation>принадлежность к российскому публичному должностному лицу</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="official" type="yes_no_t">
				<xsd:annotation>
					<xsd:documentation>признак, является ли лицо российским публичным должностным лицом</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="comment" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>должность/ комментарий </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="scan_info_t">
		<xsd:annotation>
			<xsd:documentation>Описание скан образа</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="mime_type">
				<xsd:annotation>
					<xsd:documentation>MIME тип скан образа. Для формата Злдис-Софт должен содержать значение application/x-zenith-scan</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="location" type="xsd:anyURI">
				<xsd:annotation>
					<xsd:documentation>Местоположение ресурса скан образа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="security_info_t">
		<xsd:annotation>
			<xsd:documentation>Информация о ценной бумаге</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="security_classification" type="security_classification_et">
				<xsd:annotation>
					<xsd:documentation>Вид ценной бумаги</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="security_category" type="security_category_et" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Категория ценной бумаги по предоставляемым правам</xsd:documentation>
					<!-- ===================================================
Заполняется только в случае, если вид ценной бумаги – SHAR –Акция
=================================================== -->
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="security_type" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Тип ценной бумаги</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="nominal_value" type="units_or_fraction_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Номинал</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="state_reg_num" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Государственный регистрационный номер</xsd:documentation>
					<!-- ===================================================
Если регистратор ведет учет нескольких выпусков с  одним государственным регистрационным номером в разрезе отдельных выпусков (по дате регистрации), рекомендуется в обязательном порядке указывать дату государственной регистрации
=================================================== -->
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="state_reg_date" type="xsd:date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дата государственной регистрации</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="proprietary_security_code" type="id_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Частный идентификационный код ценной бумаги</xsd:documentation>
					<!-- ===================================================
Зарезервировано для последующего использования
=================================================== -->
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="security_balance_t">
		<xsd:annotation>
			<xsd:documentation>Баланс ценных бумаг</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="security" type="security_info_t">
				<xsd:annotation>
					<xsd:documentation>Ценная бумага</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="quantity" type="security_quantity_t">
				<xsd:annotation>
					<xsd:documentation>Количество ценных бумаг на счете</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="security_quantity_t">
		<xsd:annotation>
			<xsd:documentation>Баланс ценных бумаг</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="total" type="units_or_fraction_t">
				<xsd:annotation>
					<xsd:documentation>Общее количество ценных бумаг на счете</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="blocked" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Из них обременено обязательствами и блокировками </xsd:documentation>
					<!-- ===================================================
(повторяющийся элемент)
=================================================== -->
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="quantity" type="units_or_fraction_t">
							<xsd:annotation>
								<xsd:documentation>Количество ценных бумаг в обременении</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="block_type" type="block_type_t">
							<xsd:annotation>
								<xsd:documentation>Вид обременения </xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="tax_category_t">
		<xsd:annotation>
			<xsd:documentation>статус налогоплательщика</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="tax_status_code" type="tax_status_et">
				<xsd:annotation>
					<xsd:documentation>Код статуса налогоплательщика</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="nonresident_type" type="nonresident_type_et" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>тип нерезидента (юридического лица)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="tax_exempt_indicator" type="yes_no_t">
				<xsd:annotation>
					<xsd:documentation>Признак наличия налоговой льготы</xsd:documentation>
					<!-- ===================================================
Yes – в случае наличия льготы
No – при отсутствии льготы.
=================================================== -->
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="narrative" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Основание для налоговой льготы </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="transaction_t">
		<xsd:annotation>
			<xsd:documentation>Описание операции перехода прав собственности</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="transaction_type" type="transaction_type_et" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Тип операции перехода прав собственности</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="security" type="security_info_t">
				<xsd:annotation>
					<xsd:documentation>Идентификатор ЦБ</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="quantity" type="units_or_fraction_t">
				<xsd:annotation>
					<xsd:documentation>Количество ЦБ</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="settlement_amount" type="currency_and_amount_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Сумма сделки</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="block_indicator" type="yes_no_t">
				<xsd:annotation>
					<xsd:documentation>Указание на наличие обременения</xsd:documentation>
					<!-- ===================================================
 передаточном распоряжении допускается использование только кода (No) Данный тип электронного документа не используется для передачи обремененных ценных бумаг
=================================================== -->
				</xsd:annotation>
			</xsd:element>
			<xsd:choice minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Описание обременения</xsd:documentation>
				</xsd:annotation>
				<xsd:element name="pledge_info" type="pledge_info_t">
					<xsd:annotation>
						<xsd:documentation>Описание залога</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="unpaid_info" type="unpaid_info_t">
					<xsd:annotation>
						<xsd:documentation>Описание обременения ЦБ обязательством оплаты</xsd:documentation>
						<!-- ===================================================
 Не подлежит заполнению, т.к. признак наличия обременения может быть No
=================================================== -->
					</xsd:annotation>
				</xsd:element>
			</xsd:choice>
			<xsd:element name="comment" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Для перевода ЦБ в депозит нотариуса - информация о кредиторе. Для передачи ДУП - описание доп.условий.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="based_info" type="based_info_t" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Основание для проведения операции</xsd:documentation>
					<!-- ===================================================
(повторяющийся элемент)
=================================================== -->
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="reference" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Референс поручения (идентификационный номер встречных распоряжений). Обязательно заполняется при проведении операции с участием НДЦД. Используется для выполнения квитовки встречных поручений в реестре.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="owner" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Наименование конечного владельца/ доверительного управляющего ценных бумаг. Заполняется при проведении операции с участием НДЦД. </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="transfer_agent_info_t">
		<xsd:annotation>
			<xsd:documentation>Информация о трансфер-агенте</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="ta_id" type="id_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Идентификатор трансфер-агента</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ta_info" type="juridical_info_t">
				<xsd:annotation>
					<xsd:documentation>Наименование и данные госрегистрации</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ta_legal_address" type="address_legal_t" nillable="true" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Адрес места нахождения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="units_or_fraction_t">
		<xsd:annotation>
			<xsd:documentation>Количество ценных бумаг в штуках</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="units" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>Целая часть и десятичная дробь. При наличии fraction - только целое</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="fraction" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дробная часть </xsd:documentation>
					<!-- ===================================================
Заполняется только в случае простой дроби
В случае десятичной дроби не заполняется
=================================================== -->
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="numerator" type="xsd:integer">
							<xsd:annotation>
								<xsd:documentation>Числитель</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="denominator" type="xsd:positiveInteger">
							<xsd:annotation>
								<xsd:documentation>Знаменатель</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
		<!-- =========================================================================================== -->
	</xsd:complexType>
	<xsd:complexType name="unpaid_info_t">
		<xsd:annotation>
			<xsd:documentation>Описание обременения ЦБ обязательством оплаты</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="id_unpaid" type="xsd:nonNegativeInteger" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Уникальный номер обременения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="unpaid_date" type="xsd:date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дата установки обременения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="unpaid_type" type="block_type_t">
				<xsd:annotation>
					<xsd:documentation>Вид обременения (UNPA - неоплата)</xsd:documentation>
					<!-- ===================================================
 Не подлежит заполнению, т.к. признак наличия обременения может быть No
=================================================== -->
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="security" type="security_info_t">
				<xsd:annotation>
					<xsd:documentation>Ценная бумага</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="quantity" type="units_or_fraction_t">
				<xsd:annotation>
					<xsd:documentation>Количество ЦБ, на которое установлено обременение</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="condition_unpaid" type="condition_block_t" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Условия обременения ЦБ</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="comment" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Комментарий</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="based_info" type="based_info_t" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Основание для проведения операции</xsd:documentation>
					<!-- ===================================================
(повторяющийся элемент)
=================================================== -->
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
