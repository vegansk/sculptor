	<xsd:simpleType name="account_type_et">
		<xsd:annotation>
			<xsd:documentation>Вид счета</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="ISSUE">
				<xsd:annotation>
					<xsd:documentation>Эмиссионный счет</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="EMITENT">
				<xsd:annotation>
					<xsd:documentation>Лицевой счет эмитента</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="OWNER">
				<xsd:annotation>
					<xsd:documentation>Владелец</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="NOMINEE">
				<xsd:annotation>
					<xsd:documentation>Номинальный держатель</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TRUSTEE">
				<xsd:annotation>
					<xsd:documentation>Доверительный управляющий</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="PLEDGEE">
				<xsd:annotation>
					<xsd:documentation>Залогодержатель</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UNKNOWN">
				<xsd:annotation>
					<xsd:documentation>Счет неустановленных лиц</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="NOTARY">
				<xsd:annotation>
					<xsd:documentation>Нотариус</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="RIGHTS">
				<xsd:annotation>
					<xsd:documentation>Доверительный управляющий правами</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TREASURY">
				<xsd:annotation>
					<xsd:documentation>Казначейский счет эмитента</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="DEPOSIT">
				<xsd:annotation>
					<xsd:documentation>Депозитный счет</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="NDCD">
				<xsd:annotation>
					<xsd:documentation>Номинальный держатель центрального депозитария</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="FORNOMINEE">
				<xsd:annotation>
					<xsd:documentation>Иностранный номинальный держатель</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="FORAUTHOR">
				<xsd:annotation>
					<xsd:documentation>Иностранный уполномоченный держатель</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="DEPOPROG">
				<xsd:annotation>
					<xsd:documentation>Счет депозитарных программ</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="authorised_person_type_et">
		<xsd:annotation>
			<xsd:documentation>Вид уполномоченного представителя</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="001">
				<xsd:annotation>
					<xsd:documentation>Уполномоченный представитель юридического лица по уставу</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="002">
				<xsd:annotation>
					<xsd:documentation>Единоличный исполнительный орган</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="003">
				<xsd:annotation>
					<xsd:documentation>Федеральный орган исполнительной власти, государственный орган или орган муниципального образования, имеющий право осуществлять права собственника от имени зарегистрированного лица</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="004">
				<xsd:annotation>
					<xsd:documentation>Должностное лицо уполномоченного государственного органа</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="005">
				<xsd:annotation>
					<xsd:documentation>Уполномоченный представитель по доверенности</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="006">
				<xsd:annotation>
					<xsd:documentation>Опекун</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="007">
				<xsd:annotation>
					<xsd:documentation>Попечитель</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="008">
				<xsd:annotation>
					<xsd:documentation>Родитель</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="009">
				<xsd:annotation>
					<xsd:documentation>Учредитель доверительного управления</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="010">
				<xsd:annotation>
					<xsd:documentation>Бенефициарный владелец</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="011">
				<xsd:annotation>
					<xsd:documentation>Выгодоприобретатель</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="012">
				<xsd:annotation>
					<xsd:documentation>Контролирующее лицо</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="block_type_et">
		<xsd:annotation>
			<xsd:documentation>Вид обременения ценных бумаг</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="BLOK">
				<xsd:annotation>
					<xsd:documentation>Блокирование ЦБ</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="BLOP">
				<xsd:annotation>
					<xsd:documentation>Блокирование заложенных ЦБ</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="BLON">
				<xsd:annotation>
					<xsd:documentation>Блокирование неоплаченных ЦБ</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="PLDG">
				<xsd:annotation>
					<xsd:documentation>Залог ЦБ</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="PLDN">
				<xsd:annotation>
					<xsd:documentation>Залог неоплаченных ЦБ</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UNPA">
