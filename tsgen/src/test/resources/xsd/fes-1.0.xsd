					<xsd:documentation>Выплата дивидендов в виде денежных средств</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="INCR">
				<xsd:annotation>
					<xsd:documentation>Конвертация   акций в акции с большей номинальной стоимостью   </xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="EXOF">
				<xsd:annotation>
					<xsd:documentation>Обмен ценных бумаг</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="INTR">
				<xsd:annotation>
					<xsd:documentation>Выплата купонного дохода</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="MCAL">
				<xsd:annotation>
					<xsd:documentation>Досрочное  обязательное погашение облигаций или выкуп привилегированных акций</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="MEET">
				<xsd:annotation>
					<xsd:documentation>Годовое общее собрание акционеров</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="REDM">
				<xsd:annotation>
					<xsd:documentation>Погашение номинальной стоимости облигаций/ выкуп паев/ выкуп привилегированных акций</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="SPLF">
				<xsd:annotation>
					<xsd:documentation>Конвертация при дроблении акций</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="SPLR">
				<xsd:annotation>
					<xsd:documentation>Конвертация при консолидации акций</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="XMET">
				<xsd:annotation>
					<xsd:documentation>Внеочередное собрание акционеров</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="PRII">
				<xsd:annotation>
					<xsd:documentation>Выплата купонного дохода и погашение номинальной стоимости (в том числе части номинальной  стоимости)</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="MRGR">
				<xsd:annotation>
					<xsd:documentation>Конвертация ценных бумаг при слиянии</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="OTHR">
				<xsd:annotation>
					<xsd:documentation>Иное событие</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
